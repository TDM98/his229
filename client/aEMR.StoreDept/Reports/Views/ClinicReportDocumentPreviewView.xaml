<UserControl    x:Class="aEMR.StoreDept.Reports.Views.ClinicReportDocumentPreviewView"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing"
                mc:Ignorable="d"
                d:DesignHeight="600" d:DesignWidth="950" >

    <Grid x:Name="LayoutRoot" Background="White">
        
    <dxp:DocumentPreviewControl
        Name="documentPreview1" Height="875" Width="950"
        DocumentSource="{Binding ReportModel,Mode=OneWay}"
        ScrollViewer.CanContentScroll="True" AutoShowParametersPanel="False" />        
    </Grid>
</UserControl>
