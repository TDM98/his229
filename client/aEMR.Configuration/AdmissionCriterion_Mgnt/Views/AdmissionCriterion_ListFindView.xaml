<UserControl
    x:Class="aEMR.Configuration.AdmissionCriterion_Mgnt.Views.AdmissionCriterion_ListFindView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Resources="clr-namespace:eHCMSLanguage;assembly=aEMRLanguage"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:commonctrl="clr-namespace:aEMR.Controls;assembly=aEMR.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="850"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Resources:eHCMSResources x:Key="eHCMSResources" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <DataTemplate x:Key="ColDeleteTemplate">
            <StackPanel Orientation="Horizontal">
                <Button
                    x:Name="lnkDelete"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    cal:Message.Attach="[Event Click]=[Action lnkDelete_Click($source,$eventArgs)]"
                    ToolTipService.ToolTip="Click here for delete">
                    <Button.Content>
                        <Image
                            Width="13"
                            Height="13"
                            Opacity="1"
                            Source="/aEMR.CommonViews;component/Assets/Images/Delete.png" />
                    </Button.Content>
                </Button>
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="AutoCompleteItemTemplate">
            <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                <TextBlock MinWidth="100" Text="{Binding ICD10Code, Mode=OneWay}" />
                <TextBlock Text="{Binding DiseaseNameVN, Mode=OneWay}" />
            </StackPanel>
        </DataTemplate>
    </UserControl.Resources>
    <Grid
        x:Name="LayoutRoot"
        Margin="5"
        Background="White">
        <xctk:BusyIndicator BusyContent="{Binding DlgBusyContent}" IsBusy="{Binding DlgIsBusyFlag}">
            <TabControl>
                <TabItem Header="Tiêu chuẩn nhập viện">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <TextBlock
                                Margin="0,10,0,5"
                                HorizontalAlignment="Center"
                                FontSize="14"
                                FontWeight="Bold"
                                Foreground="#FF0084FF"
                                Text="Danh mục tiêu chuẩn" />
                        </Grid>
                        <StackPanel
                            Grid.Row="1"
                            Margin="0,5"
                            Orientation="Horizontal"
                            Visibility="{Binding hplAddNewVisible, Mode=TwoWay}">
                            <Button
                                Name="hplAddNewAC"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                cal:Message.Attach="[Event Click]=[Action hplAddNewAC_Click()]"
                                FontSize="12"
                                FontWeight="Bold"
                                Foreground="#FF0084FF"
                                Style="{StaticResource MainFrame.HyperlinkButton.01}"
                                ToolTipService.ToolTip="Tạo Mới Tiêu Chuẩn"
                                Visibility="{Binding bhplAddNew, Converter={StaticResource BooleanToVisibility}}">
                                <Button.Content>
                                    <StackPanel Orientation="Horizontal">
                                        <Image
                                            Width="13"
                                            Height="13"
                                            Opacity="1"
                                            Source="/aEMR.CommonViews;component/Assets/Images/page-add-icon2.png" />
                                        <TextBlock Text="Tạo mới tiêu chuẩn" />
                                    </StackPanel>
                                </Button.Content>
                            </Button>
                        </StackPanel>
                        <Border
                            Grid.Row="2"
                            Margin="0,10,0,5"
                            Background="AliceBlue"
                            BorderBrush="#C1D2E6"
                            BorderThickness="1"
                            CornerRadius="3">
                            <StackPanel>
                                <Border
                                    Margin="10,-10,0,0"
                                    HorizontalAlignment="Left"
                                    Background="AliceBlue"
                                    BorderBrush="#6F8AA9"
                                    BorderThickness="1">
                                    <TextBlock
                                        Margin="10,0,10,0"
                                        FontWeight="Bold"
                                        Text="{Binding G1174_G1_TimKiem, Source={StaticResource eHCMSResources}}" />
                                </Border>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="120" />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Label
                                        Grid.Row="1"
                                        Grid.Column="0"
                                        Margin="0,2,5,2"
                                        HorizontalAlignment="Right"
                                        Content="Tên tiêu chuẩn"
                                        FontWeight="Normal" />
                                    <TextBox
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        Width="400"
                                        Margin="0,2,5,2"
                                        HorizontalAlignment="Left"
                                        Text="{Binding SearchCriteriaAC, Mode=TwoWay}" />

                                    <Button
                                        x:Name="btSearchAC"
                                        Grid.Row="2"
                                        Grid.Column="1"
                                        Width="60"
                                        Margin="0,0,5,5"
                                        HorizontalAlignment="Left"
                                        Visibility="{Binding bbtSearch, Converter={StaticResource BooleanToVisibility}}">
                                        <StackPanel Orientation="Horizontal">
                                            <Image
                                                Width="20"
                                                Height="18"
                                                VerticalAlignment="Center"
                                                Source="/aEMR.CommonViews;component/Assets/Images/1306555029_find.png" />
                                            <TextBlock VerticalAlignment="Center" Text="{Binding G0981_G1_Tim, Source={StaticResource eHCMSResources}}" />
                                        </StackPanel>
                                    </Button>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border
                            Grid.Row="3"
                            Margin="0,10,0,5"
                            Background="AliceBlue"
                            BorderBrush="#C1D2E6"
                            BorderThickness="1"
                            CornerRadius="3">
                            <StackPanel>
                                <Border
                                    Margin="10,-10,0,0"
                                    HorizontalAlignment="Left"
                                    Background="AliceBlue"
                                    BorderBrush="#6F8AA9"
                                    BorderThickness="1">
                                    <TextBlock
                                        Margin="10,0,10,0"
                                        FontWeight="Bold"
                                        Text="Danh sách tiêu chuẩn" />
                                </Border>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition Height="25" />
                                    </Grid.RowDefinitions>
                                    <commonctrl:ReadOnlyDataGrid
                                        Name="dtgListAC"
                                        Grid.Row="0"
                                        Margin="0,2"
                                        cal:Message.Attach="[Event SelectionChanged] = [Action AdmissionCriterionSelectionChanged()];"
                                        AutoGenerateColumns="False"
                                        CanUserAddRows="False"
                                        ColumnHeaderStyle="{StaticResource MainFrame.DataGridColumnHeaderStyle}"
                                        HeadersVisibility="All"
                                        HorizontalScrollBarVisibility="Auto"
                                        IsReadOnly="True"
                                        ItemsSource="{Binding ObjAdmissionCriterion_Paging, Mode=TwoWay}"
                                        SelectedItem="{Binding SelectedAdmissionCriterion}"
                                        Style="{StaticResource MainFrame.DataGridStyle}"
                                        VerticalScrollBarVisibility="Visible">
                                        <DataGrid.Columns>
                                            <DataGridTemplateColumn x:Name="colTemplate" Header="">
                                                <DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <StackPanel Orientation="Horizontal">
                                                            <Button
                                                                x:Name="hplEdit"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                cal:Message.Attach="[Event Click]=[Action hplEditAC_Click($datacontext)]                                                              ;[Event Loaded]=[Action hplEdit_Loaded($source)]"
                                                                Style="{StaticResource MainFrame.HyperlinkButton.01}"
                                                                ToolTipService.ToolTip="{Binding T1484_G1_HChinh, Source={StaticResource eHCMSResources}}">
                                                                <Button.Content>
                                                                    <Image
                                                                        Width="15"
                                                                        Height="15"
                                                                        Opacity="1"
                                                                        Source="/aEMR.CommonViews;component/Assets/Images/edit-icon.png" />
                                                                </Button.Content>
                                                            </Button>
                                                            <Button
                                                                x:Name="hplDelete"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                cal:Message.Attach="[Event Click]=[Action hplDeleteAC_Click($datacontext)]                                                             ;[Event Loaded]=[Action hplDelete_Loaded($source)]"
                                                                Style="{StaticResource MainFrame.HyperlinkButton.01}"
                                                                ToolTipService.ToolTip="{Binding G2617_G1_Xoa, Source={StaticResource eHCMSResources}}">
                                                                <Button.Content>
                                                                    <Image
                                                                        Width="13"
                                                                        Height="13"
                                                                        Opacity="1"
                                                                        Source="/aEMR.CommonViews;component/Assets/Images/Delete.png" />
                                                                </Button.Content>
                                                            </Button>
                                                        </StackPanel>
                                                    </DataTemplate>
                                                </DataGridTemplateColumn.CellTemplate>
                                            </DataGridTemplateColumn>
                                            <!--<DataGridTextColumn
                                        x:Name="col0"
                                        Binding="{Binding AdmissionCriteriaCode, Mode=TwoWay}"
                                        Header="Mã tiêu chí" />-->

                                            <DataGridTextColumn Binding="{Binding AdmissionCriterionName, Mode=TwoWay}" Header="Tên tiêu chuẩn">
                                                <DataGridTextColumn.ElementStyle>
                                                    <Style>
                                                        <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
                                                    </Style>
                                                </DataGridTextColumn.ElementStyle>
                                            </DataGridTextColumn>
                                            <DataGridCheckBoxColumn Binding="{Binding IsActive, Mode=TwoWay}" Header="{Binding K2846_G1_DangDung, Source={StaticResource eHCMSResources}}" />
                                            <!--<DataGridTextColumn Binding="{Binding SymptomTypeName, Mode=TwoWay}" Header="Loại tiêu chuẩn" />-->
                                        </DataGrid.Columns>
                                    </commonctrl:ReadOnlyDataGrid>
                                    <commonctrl:DataPager
                                        Grid.Row="1"
                                        VerticalAlignment="Bottom"
                                        PageSize="9"
                                        Source="{Binding ObjAdmissionCriterion_Paging, Mode=TwoWay}" />
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Grid Grid.Row="4">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Border
                                Margin="0,10,0,5"
                                Background="AliceBlue"
                                BorderBrush="#C1D2E6"
                                BorderThickness="1"
                                CornerRadius="3">
                                <StackPanel>
                                    <Border
                                        Margin="10,-10,0,0"
                                        HorizontalAlignment="Left"
                                        Background="AliceBlue"
                                        BorderBrush="#6F8AA9"
                                        BorderThickness="1">
                                        <TextBlock
                                            Margin="10,0,10,0"
                                            FontWeight="Bold"
                                            Text="Danh sách ICD kèm theo" />
                                    </Border>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="32" />
                                            <RowDefinition Height="34" />
                                            <!--<RowDefinition Height="27" />-->
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <StackPanel
                                            Grid.Row="0"
                                            Margin="5"
                                            Orientation="Horizontal">
                                            <TextBlock
                                                Margin="0,0,5,0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.02}"
                                                Text="{Binding T1793_G1_ICD10, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                                            <commonctrl:AxAutoComplete
                                                Height="22"
                                                cal:Message.Attach="[Event Loaded] = [Action AcbICD10Code_Loaded($source, $eventArgs)];                                                                          [Event Populating] = [Action aucICD10_Populating($source, $eventArgs)];                                                                          [Event DropDownClosed] = [Action AxAutoComplete_DropDownClosed($source, $eventArgs)];                                                                          [Event DropDownClosing] = [Action AxAutoComplete_DropDownClosing($source, $eventArgs)]"
                                                FilterMode="None"
                                                IsDropDownOpen="True"
                                                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                                                ItemTemplate="{StaticResource AutoCompleteItemTemplate}"
                                                MinimumPopulateDelay="300"
                                                MinimumPrefixLength="1"
                                                Style="{StaticResource AxAutoCompleteMyStyle}"
                                                ValueMemberBinding="{Binding ICD10Code}"
                                                ValueMemberPath="ICD10Code" />
                                            <TextBox
                                                Width="400"
                                                Height="22"
                                                Margin="5,0,0,0"
                                                Text="{Binding BasicDiagTreatment}" />
                                        </StackPanel>
                                        <StackPanel Grid.Row="1" Orientation="Horizontal">
                                            <Button
                                                x:Name="butAdd"
                                                Grid.Row="1"
                                                Width="80"
                                                Height="22"
                                                Margin="5"
                                                HorizontalAlignment="Left"
                                                Content="{Binding G0156_G1_Them, Source={StaticResource eHCMSResources}}"
                                                Style="{StaticResource MainFrame.Button}" />
                                            <Button
                                                x:Name="butSave"
                                                Grid.Row="1"
                                                Width="80"
                                                Height="22"
                                                Margin="5"
                                                HorizontalAlignment="Left"
                                                Content="{Binding T2937_G1_Luu, Source={StaticResource eHCMSResources}}"
                                                Style="{StaticResource MainFrame.Button}" />
                                            <Button
                                                x:Name="BtnExportExcelICD"
                                                Grid.Row="1"
                                                Width="80"
                                                Height="22"
                                                Margin="5"
                                                HorizontalAlignment="Left"
                                                Content="{Binding G2849_G1_XuatExcel, Source={StaticResource eHCMSResources}}"
                                                Style="{StaticResource MainFrame.Button}" />
                                        </StackPanel>

                                        <!--<TextBlock
                                            Grid.Row="2"
                                            Height="18"
                                            Margin="5,10,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            Foreground="#FFE65D0F"
                                            Style="{StaticResource TextBlock.Heading.02}"
                                            Text="{Binding Z2622_G1_DanhSachICD10, Source={StaticResource eHCMSResources}}" />-->
                                        <Border
                                            Grid.Row="3"
                                            Grid.Column="1"
                                            Margin="1,1,1,1"
                                            VerticalAlignment="Stretch"
                                            ScrollViewer.VerticalScrollBarVisibility="Auto"
                                            Style="{StaticResource LightBlueBorderStyle}">
                                            <DataGrid
                                                x:Name="griddrug"
                                                MaxHeight="140"
                                                Margin="2"
                                                AutoGenerateColumns="False"
                                                ColumnHeaderStyle="{StaticResource MainFrame.DataGridColumnHeaderStyle}"
                                                HorizontalScrollBarVisibility="Auto"
                                                IsReadOnly="True"
                                                ItemsSource="{Binding allAdmissionCriterionAttachICD, Mode=TwoWay}"
                                                SelectedItem="{Binding selectedAdmissionCriterionAttachICD, Mode=TwoWay}"
                                                Style="{StaticResource MainFrame.DataGridStyle}"
                                                VerticalScrollBarVisibility="Auto">
                                                <DataGrid.Columns>
                                                    <DataGridTemplateColumn CellTemplate="{StaticResource ColDeleteTemplate}" />
                                                    <DataGridTextColumn
                                                        x:Name="colMa"
                                                        MinWidth="100"
                                                        Binding="{Binding ICD10Code}"
                                                        Header="{Binding T3185_G1_Ma, Source={StaticResource eHCMSResources}}" />
                                                    <DataGridTextColumn
                                                        x:Name="colTen"
                                                        MinWidth="200"
                                                        Binding="{Binding DiseaseNameVN}"
                                                        Header="{Binding T0829_G1_Ten, Source={StaticResource eHCMSResources}}" />
                                                </DataGrid.Columns>
                                            </DataGrid>
                                        </Border>
                                    </Grid>
                                </StackPanel>
                            </Border>
                            <Border
                                Grid.Column="1"
                                Margin="0,10,0,5"
                                Background="AliceBlue"
                                BorderBrush="#C1D2E6"
                                BorderThickness="1"
                                CornerRadius="3">
                                <StackPanel>
                                    <Border
                                        Margin="10,-10,0,0"
                                        HorizontalAlignment="Left"
                                        Background="AliceBlue"
                                        BorderBrush="#6F8AA9"
                                        BorderThickness="1">
                                        <TextBlock
                                            Margin="10,0,10,0"
                                            FontWeight="Bold"
                                            Text="Danh sách nhóm CLS" />
                                    </Border>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="32" />
                                            <RowDefinition Height="34" />
                                            <!--<RowDefinition Height="27" />-->
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <StackPanel
                                            Grid.Row="0"
                                            Margin="5"
                                            Orientation="Horizontal">
                                            <TextBlock
                                                Margin="0,0,5,0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.02}"
                                                Text="Nhóm CLS" />
                                            <commonctrl:AxAutoComplete
                                                Width="300"
                                                Height="22"
                                                cal:Message.Attach="[Event Loaded] = [Action AcbGroupPCLs_Loaded($source, $eventArgs)];                                                                          [Event Populating] = [Action aucGroupPCLs_Populating($source, $eventArgs)];                                                                          [Event DropDownClosed] = [Action AxAutoComplete_GroupPCLs_DropDownClosed($source, $eventArgs)];                                                                          [Event DropDownClosing] = [Action AxAutoComplete_GroupPCLs_DropDownClosing($source, $eventArgs)]"
                                                FilterMode="None"
                                                IsDropDownOpen="True"
                                                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                                                MinimumPopulateDelay="300"
                                                MinimumPrefixLength="1"
                                                Style="{StaticResource AxAutoCompleteMyStyle}"
                                                ValueMemberBinding="{Binding GroupPCLName}"
                                                ValueMemberPath="GroupPCLName">
                                                <commonctrl:AxAutoComplete.ItemTemplate>
                                                    <DataTemplate>
                                                        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                                            <!--<TextBlock MinWidth="100" Text="{Binding ICD10Code, Mode=OneWay}" />-->
                                                            <TextBlock Text="{Binding GroupPCLName, Mode=OneWay}" />
                                                        </StackPanel>
                                                    </DataTemplate>
                                                </commonctrl:AxAutoComplete.ItemTemplate>
                                            </commonctrl:AxAutoComplete>

                                            <!--<TextBox
                                                Width="400"
                                                Height="22"
                                                Margin="5,0,0,0"
                                                Text="{Binding BasicDiagTreatment}" />-->
                                        </StackPanel>
                                        <StackPanel Grid.Row="1" Orientation="Horizontal">
                                            <Button
                                                x:Name="butAddGroupPCL"
                                                Grid.Row="1"
                                                Width="80"
                                                Height="22"
                                                Margin="5"
                                                HorizontalAlignment="Left"
                                                Content="{Binding G0156_G1_Them, Source={StaticResource eHCMSResources}}"
                                                Style="{StaticResource MainFrame.Button}" />
                                            <Button
                                                x:Name="butSaveGroupPCL"
                                                Grid.Row="1"
                                                Width="80"
                                                Height="22"
                                                Margin="5"
                                                HorizontalAlignment="Left"
                                                Content="{Binding T2937_G1_Luu, Source={StaticResource eHCMSResources}}"
                                                Style="{StaticResource MainFrame.Button}" />
                                            <Button
                                                x:Name="BtnExportExcelGroupPCL"
                                                Grid.Row="1"
                                                Width="80"
                                                Height="22"
                                                Margin="5"
                                                HorizontalAlignment="Left"
                                                Content="{Binding G2849_G1_XuatExcel, Source={StaticResource eHCMSResources}}"
                                                Style="{StaticResource MainFrame.Button}" />
                                        </StackPanel>

                                        <!--<TextBlock
                                            Grid.Row="2"
                                            Height="18"
                                            Margin="5,10,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            Foreground="#FFE65D0F"
                                            Style="{StaticResource TextBlock.Heading.02}"
                                            Text="Danh sách nhóm CLS" />-->
                                        <Border
                                            Grid.Row="3"
                                            Grid.Column="1"
                                            Margin="1,1,1,1"
                                            VerticalAlignment="Stretch"
                                            ScrollViewer.VerticalScrollBarVisibility="Auto"
                                            Style="{StaticResource LightBlueBorderStyle}">
                                            <DataGrid
                                                MaxHeight="140"
                                                Margin="2"
                                                AutoGenerateColumns="False"
                                                ColumnHeaderStyle="{StaticResource MainFrame.DataGridColumnHeaderStyle}"
                                                HorizontalScrollBarVisibility="Auto"
                                                IsReadOnly="True"
                                                ItemsSource="{Binding allAdmissionCriterionAttachGroupPCL, Mode=TwoWay}"
                                                SelectedItem="{Binding selectedAdmissionCriterionAttachGroupPCL, Mode=TwoWay}"
                                                Style="{StaticResource MainFrame.DataGridStyle}"
                                                VerticalScrollBarVisibility="Auto">
                                                <DataGrid.Columns>
                                                    <DataGridTemplateColumn>
                                                        <DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Button
                                                                    x:Name="lnkDelete"
                                                                    HorizontalAlignment="Center"
                                                                    VerticalAlignment="Center"
                                                                    cal:Message.Attach="[Event Click]=[Action lnkDeleteGroupPCLs_Click($source,$eventArgs)]"
                                                                    ToolTipService.ToolTip="Click here for delete">
                                                                    <Button.Content>
                                                                        <Image
                                                                            Width="13"
                                                                            Height="13"
                                                                            Opacity="1"
                                                                            Source="/aEMR.CommonViews;component/Assets/Images/Delete.png" />
                                                                    </Button.Content>
                                                                </Button>
                                                            </DataTemplate>
                                                        </DataGridTemplateColumn.CellTemplate>
                                                    </DataGridTemplateColumn>
                                                    <!--<DataGridTextColumn
                                                        MinWidth="100"
                                                        Binding="{Binding ICD10Code}"
                                                        Header="{Binding T3185_G1_Ma, Source={StaticResource eHCMSResources}}" />-->
                                                    <DataGridTextColumn
                                                        MinWidth="200"
                                                        Binding="{Binding GroupPCLName}"
                                                        Header="{Binding T0829_G1_Ten, Source={StaticResource eHCMSResources}}" />
                                                </DataGrid.Columns>
                                            </DataGrid>
                                        </Border>
                                    </Grid>
                                </StackPanel>
                            </Border>
                        </Grid>

                        <Border
                            Grid.Row="5"
                            Margin="0,10,0,5"
                            Background="AliceBlue"
                            BorderBrush="#C1D2E6"
                            BorderThickness="1"
                            CornerRadius="3">
                            <StackPanel>
                                <Border
                                    Margin="10,-10,0,0"
                                    HorizontalAlignment="Left"
                                    Background="AliceBlue"
                                    BorderBrush="#6F8AA9"
                                    BorderThickness="1">
                                    <TextBlock
                                        Margin="10,0,10,0"
                                        FontWeight="Bold"
                                        Text="Danh sách triệu chứng" />
                                </Border>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="27" />
                                            <RowDefinition Height="32" />
                                            <RowDefinition Height="34" />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <TextBlock
                                            Grid.Row="0"
                                            Height="18"
                                            Margin="5,10,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            Foreground="#FFE65D0F"
                                            Style="{StaticResource TextBlock.Heading.02}"
                                            Text="Danh sách triệu chứng bắt buộc" />
                                        <StackPanel
                                            Grid.Row="1"
                                            Margin="5"
                                            Orientation="Horizontal">
                                            <TextBlock
                                                Margin="0,0,5,0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.02}"
                                                Text="Triệu chứng" />
                                            <commonctrl:AxAutoComplete
                                                Width="300"
                                                Height="22"
                                                cal:Message.Attach="[Event Loaded] = [Action AcbSymptom_Required_Loaded($source, $eventArgs)];                                                                          [Event Populating] = [Action aucSymptom_Required_Populating($source, $eventArgs)];                                                                          [Event DropDownClosed] = [Action AxAutoComplete_Symptom_Required_DropDownClosed($source, $eventArgs)];                                                                          [Event DropDownClosing] = [Action AxAutoComplete_Symptom_Required_DropDownClosing($source, $eventArgs)]"
                                                FilterMode="None"
                                                IsDropDownOpen="True"
                                                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                                                MinimumPopulateDelay="300"
                                                MinimumPrefixLength="1"
                                                Style="{StaticResource AxAutoCompleteMyStyle}"
                                                ValueMemberBinding="{Binding SymptomCategoryName}"
                                                ValueMemberPath="SymptomCategoryName">
                                                <commonctrl:AxAutoComplete.ItemTemplate>
                                                    <DataTemplate>
                                                        <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                                            <!--<TextBlock MinWidth="100" Text="{Binding ICD10Code, Mode=OneWay}" />-->
                                                            <TextBlock Text="{Binding SymptomCategoryName, Mode=OneWay}" />
                                                        </StackPanel>
                                                    </DataTemplate>
                                                </commonctrl:AxAutoComplete.ItemTemplate>
                                            </commonctrl:AxAutoComplete>
                                            <!--<TextBox
                                                Width="400"
                                                Height="22"
                                                Margin="5,0,0,0"
                                                Text="{Binding BasicDiagTreatment}" />-->
                                        </StackPanel>
                                        <StackPanel Grid.Row="2" Orientation="Horizontal">
                                            <Button
                                                x:Name="butAddSCItem_Required"
                                                Grid.Row="1"
                                                Width="80"
                                                Height="22"
                                                Margin="5"
                                                HorizontalAlignment="Left"
                                                Content="{Binding G0156_G1_Them, Source={StaticResource eHCMSResources}}"
                                                Style="{StaticResource MainFrame.Button}" />
                                            <Button
                                                x:Name="butSaveSCItem_Required"
                                                Grid.Row="1"
                                                Width="80"
                                                Height="22"
                                                Margin="5"
                                                HorizontalAlignment="Left"
                                                Content="{Binding T2937_G1_Luu, Source={StaticResource eHCMSResources}}"
                                                Style="{StaticResource MainFrame.Button}" />
