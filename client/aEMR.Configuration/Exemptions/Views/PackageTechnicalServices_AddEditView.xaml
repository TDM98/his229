<UserControl
    x:Class="aEMR.Configuration.Exemptions.Views.PackageTechnicalServices_AddEditView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Controls="clr-namespace:aEMR.Controls;assembly=aEMR.Controls"
    xmlns:Converters="clr-namespace:aEMR.Common.Converters;assembly=aEMR.Common"
    xmlns:Resources="clr-namespace:eHCMSLanguage;assembly=aEMRLanguage"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="500"
    d:DesignWidth="800"
    Background="White"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Resources:eHCMSResources x:Key="eHCMSResources" />
        <Converters:InverseBoolConverter x:Key="InverseBoolConverter" />
        <Converters:DecimalToPercentConverter x:Key="DecimalToPercentConverter" />
        <Controls:KeySearchableComboBox x:Key="KeySearchableComboBox" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
    </UserControl.Resources>
    <xctk:BusyIndicator BusyContent="{Binding DlgBusyContent}" IsBusy="{Binding DlgIsBusyFlag}">
        <TabControl>
            <TabItem Header="Gói DVKT">
                <Grid Grid.Row="1" Margin="4">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="140" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <TextBlock
                        Grid.Row="1"
                        Margin="5,0"
                        VerticalAlignment="Center"
                        Style="{StaticResource TextBlock.Heading.02}"
                        Text="{Binding T0829_G1_Ten, Source={StaticResource eHCMSResources}}" />
                    <TextBox
                        Grid.Row="1"
                        Grid.Column="1"
                        Grid.ColumnSpan="2"
                        Margin="5,5,0,5"
                        Style="{StaticResource MainFrame.TextBox}"
                        Text="{Binding ObjPackageTechnicalServices_Current.Title, Mode=TwoWay}" />
                    <StackPanel
                        Grid.Row="8"
                        Grid.Column="1"
                        Margin="5,5,0,5"
                        Orientation="Horizontal">
                        <Button
                            x:Name="btSave"
                            MinWidth="80"
                            Padding="5,0"
                            Content="{Binding T2937_G1_Luu, Source={StaticResource eHCMSResources}}"
                            IsEnabled="{Binding IsViewOld, Converter={StaticResource InverseBoolConverter}}"
                            Style="{StaticResource MainFrame.Button}" />
                    </StackPanel>
                </Grid>
            </TabItem>
            <TabItem Header="Dịch vụ">
                <Grid x:Name="GridMain" Margin="5">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition MinWidth="350" />
                        <ColumnDefinition Width="40" />
                        <ColumnDefinition MinWidth="350" />
                    </Grid.ColumnDefinitions>
                    <Border
                        Grid.Row="0"
                        Grid.Column="0"
                        Grid.ColumnSpan="3"
                        Margin="0,10,0,5"
                        Background="AliceBlue"
                        BorderBrush="#C1D2E6"
                        BorderThickness="1"
                        CornerRadius="3">
                        <StackPanel>
                            <Border
                                Margin="10,-10,0,0"
                                HorizontalAlignment="Left"
                                Background="AliceBlue"
                                BorderBrush="#6F8AA9"
                                BorderThickness="1">
                                <TextBlock
                                    Margin="10,0,10,0"
                                    FontWeight="Bold"
                                    Text="{Binding G1219_G1_TimKiemDV, Source={StaticResource eHCMSResources}}" />
                            </Border>
                            <Grid Margin="0,5,0,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition Width="220" />
                                    <ColumnDefinition Width="50*" />
                                    <ColumnDefinition Width="128*" />
                                    <ColumnDefinition Width="268*" />
                                    <ColumnDefinition Width="122*" />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Column="0"
                                    Margin="0,2,5,2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    FontWeight="Normal"
                                    Text="{Binding T2707_G1_LoaiDV, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <Controls:AxComboBox
                                    Name="cboMedicalServiceTypesSubTractPCL"
                                    Grid.Column="1"
                                    Margin="0,2,5,2"
                                    DisplayMemberPath="MedicalServiceTypeName"
                                    FontWeight="Normal"
                                    Foreground="Black"
                                    ItemsSource="{Binding ObjRefMedicalServiceTypes_GetAll, Mode=OneWay}"
                                    SelectedItemEx="{Binding ObjRefMedicalServiceTypeSelected, Mode=TwoWay}"
                                    SelectedValuePath="MedicalServiceTypeID"
                                    Style="{StaticResource MainFrame.ComboBox}" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Margin="0,2,5,2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    FontWeight="Normal"
                                    Text="{Binding T3320_G1_MaDV, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <TextBox
                                    x:Name="tbMedServiceCode"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,2,5,2"
                                    MaxLength="15"
                                    Text="{Binding RefMedSerItemsSearchCriteria.MedServiceCode, Mode=TwoWay}" />

                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="3"
                                    Margin="0,2,5,2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    FontWeight="Normal"
                                    Text="{Binding A1164_G1_TenDV, Source={StaticResource eHCMSResources}}" />
                                <TextBox
                                    x:Name="tbMedServiceName"
                                    Grid.Row="0"
                                    Grid.Column="4"
                                    Margin="0,2,5,2"
                                    MaxLength="128"
                                    Text="{Binding RefMedSerItemsSearchCriteria.MedServiceName, Mode=TwoWay}" />

                                <Button
                                    x:Name="btSearch"
                                    Grid.Row="1"
                                    Grid.Column="4"
                                    Width="60"
                                    Margin="0,0,0,5"
                                    HorizontalAlignment="Left"
                                    Style="{StaticResource MainFrame.Button}">
                                    <StackPanel Orientation="Horizontal">
                                        <Image
                                            Width="20"
                                            Height="18"
                                            VerticalAlignment="Center"
                                            Source="/aEMR.CommonViews;component/Assets/Images/1306555029_find.png" />
                                        <TextBlock VerticalAlignment="Center" Text="{Binding G0981_G1_Tim, Source={StaticResource eHCMSResources}}" />
                                    </StackPanel>
                                </Button>
                            </Grid>
                        </StackPanel>
                    </Border>
                    <Border
                        Grid.Row="1"
                        Grid.Column="0"
                        MaxWidth="500"
                        Margin="0,10,0,5"
                        Background="AliceBlue"
                        BorderBrush="#C1D2E6"
                        BorderThickness="1"
                        CornerRadius="3">
                        <StackPanel>
                            <Border
                                Margin="10,-10,0,0"
                                HorizontalAlignment="Left"
                                Background="AliceBlue"
                                BorderBrush="#6F8AA9"
                                BorderThickness="1">
                                <TextBlock
                                    Margin="10,0,10,0"
                                    FontWeight="Bold"
                                    Text="{Binding K2971_G1_DSDVCuaBV, Source={StaticResource eHCMSResources}}" />
                            </Border>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition Height="25" />
                                    <RowDefinition Height="25" />
                                </Grid.RowDefinitions>
                                <Controls:ReadOnlyDataGrid
                                    Name="dtList"
                                    Grid.Row="1"
                                    Height="295"
                                    Margin="0,2"
                                    cal:Message.Attach="[Event DblClick]=[Action MedServiceItemDoubleClick()]"
                                    AutoGenerateColumns="False"
                                    ColumnHeaderStyle="{StaticResource MainFrame.DataGridColumnHeaderStyle}"
                                    HeadersVisibility="All"
                                    HorizontalScrollBarVisibility="Auto"
                                    IsReadOnly="True"
                                    ItemsSource="{Binding ObjMedServiceItems_Paging, Mode=OneWay}"
                                    LoadingRow="dtgList_LoadingRow"
                                    SelectedItem="{Binding curRefMedicalServiceItem, Mode=TwoWay}"
                                    Style="{StaticResource MainFrame.DataGridStyle}"
                                    VerticalScrollBarVisibility="auto">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Binding="{Binding MedServiceCode}" Header="{Binding T3364_G1_MaDV, Source={StaticResource eHCMSResources}}" />
                                        <DataGridTextColumn Binding="{Binding MedServiceName}" Header="{Binding T0844_G1_TenDV, Source={StaticResource eHCMSResources}}" />
                                        <DataGridTextColumn Binding="{Binding ObjV_RefMedServiceItemsUnit.ObjectValue}" Header="{Binding K3966_G1_Dvt, Source={StaticResource eHCMSResources}}" />
                                        <DataGridTemplateColumn x:Name="ColNormalPrice" Header="{Binding K3591_G1_DGia, Source={StaticResource eHCMSResources}}">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock
                                                        HorizontalAlignment="Right"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding NormalPrice, StringFormat=N0}" />
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn x:Name="ColVAT" Header="{Binding G2156_G1_VAT, Source={StaticResource eHCMSResources}}">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock
                                                        HorizontalAlignment="Right"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding VATRate}" />
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn x:Name="ColPriceForHIPatient" Header="Giá BN BH">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock
                                                        HorizontalAlignment="Right"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding PriceForHIPatient, StringFormat=N0}"
                                                        ToolTipService.ToolTip="Giá Bệnh Nhân Bảo Hiểm" />
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn x:Name="ColHIAllowedPrice" Header="{Binding K1099_G1_BHChoPhep, Source={StaticResource eHCMSResources}}">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock
                                                        HorizontalAlignment="Right"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding HIAllowedPrice, StringFormat=N0}" />
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTextColumn
                                            x:Name="Lcol5"
                                            Binding="{Binding EffectiveDate, StringFormat='dd/MM/yyyy'}"
                                            Header="{Binding K0914_G1_ApDungTu, Source={StaticResource eHCMSResources}}" />
                                        <DataGridTextColumn
                                            x:Name="Lcol6"
                                            Binding="{Binding ObjStaffID.FullName}"
                                            Header="{Binding N0289_G1_NhVienLapGia, Source={StaticResource eHCMSResources}}" />
                                        <DataGridTextColumn
                                            x:Name="Lcol8"
                                            Binding="{Binding Note}"
                                            Header="{Binding G1183_G1_Ghichu, Source={StaticResource eHCMSResources}}" />
                                        <DataGridTextColumn Binding="{Binding ObjMedicalServiceTypeID.MedicalServiceTypeName}" Header="{Binding T2707_G1_LoaiDV, Source={StaticResource eHCMSResources}}" />
                                    </DataGrid.Columns>
                                </Controls:ReadOnlyDataGrid>
                                <StackPanel
                                    Grid.Row="2"
                                    Margin="0,2,40,0"
                                    HorizontalAlignment="Right"
                                    Orientation="Horizontal">
                                    <TextBlock VerticalAlignment="Center" Text="{Binding S0583_G1_SLg, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                    <TextBox
                                        x:Name="ServiceQty"
                                        Width="40"
                                        Height="22"
                                        Margin="5,0,5,0"
                                        VerticalContentAlignment="Center"
                                        Text="{Binding ServiceQty, Mode=TwoWay}"
                                        TextAlignment="Right" />
                                </StackPanel>
                                <Controls:DataPager
                                    Grid.Row="3"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Bottom"
                                    PageSize="{Binding PageSize}"
                                    Source="{Binding ObjMedServiceItems_Paging, Mode=OneWay}"
                                    Visibility="Collapsed" />
                            </Grid>
                        </StackPanel>
                    </Border>

                    <StackPanel
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="0,50,0,0"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <Button
                            x:Name="btAddChooseMedService"
                            Width="26"
                            Height="40"
                            ToolTipService.ToolTip="{Binding eHCMSResources.G0156_G1_Them, Source={StaticResource eHCMSResources}}"
                            Visibility="{Binding bbtAdd, Converter={StaticResource BooleanToVisibility}}">
                            <Image
                                Width="32"
                                Height="32"
                                Margin="-5,0,0,0"
                                HorizontalAlignment="Left"
                                Opacity="1"
                                Source="/aEMR.CommonViews;component/Assets/Images/navigate-right-icon48x48.png" />
                        </Button>
                        <Label Height="10" Content="" />
                        <Button
                            x:Name="btSaveMedServiceItems"
                            Width="28"
                            Height="40"
                            Margin="0,0,0,10"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            ToolTipService.ToolTip="{Binding T2937_G1_Luu, Source={StaticResource eHCMSResources}}"
                            Visibility="{Binding bbtAdd, Converter={StaticResource BooleanToVisibility}}">
                            <Image Opacity="1" Source="/aEMR.CommonViews;component/Assets/Images/saveanddown24x24border.png" />
                        </Button>
                    </StackPanel>
                    <Border
                        Grid.Row="1"
                        Grid.Column="2"
                        Margin="0,10,0,5"
                        Background="AliceBlue"
                        BorderBrush="#C1D2E6"
                        BorderThickness="1"
                        CornerRadius="3">
                        <StackPanel>
                            <Border
                                Margin="10,-10,0,0"
                                HorizontalAlignment="Left"
                                Background="AliceBlue"
                                BorderBrush="#6F8AA9"
                                BorderThickness="1">
                                <TextBlock
                                    Margin="10,0,10,0"
                                    FontWeight="Bold"
                                    Text="Danh sách dịch vụ" />
                            </Border>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <StackPanel
                                    Grid.Row="0"
                                    Margin="0,5"
                                    Orientation="Horizontal">
                                    <Grid Name="Grid">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                    </Grid>
                                </StackPanel>
                                <DataGrid
                                    Name="List"
                                    Grid.Row="1"
                                    Height="310"
                                    Margin="0,2"
                                    AutoGenerateColumns="False"
                                    ColumnHeaderStyle="{StaticResource MainFrame.DataGridColumnHeaderStyle}"
                                    HeadersVisibility="All"
                                    HorizontalScrollBarVisibility="Auto"
                                    IsReadOnly="True"
                                    ItemsSource="{Binding allMedServiceItems.TempObject, Mode=OneWay}"
                                    LoadingRow="dtgList_LoadingRow"
                                    SelectedItem="{Binding curMedServiceItems, Mode=TwoWay}"
                                    Style="{StaticResource MainFrame.DataGridStyle}"
                                    VerticalScrollBarVisibility="Auto">
                                    <DataGrid.Columns>
                                        <DataGridTemplateColumn x:Name="colTemplate" Header="">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <StackPanel Orientation="Horizontal">
                                                        <Button
                                                            x:Name="hplDeleteService"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            cal:Message.Attach="[Event Click]=[Action hplDeleteService_Click($datacontext)]                                                                                     ;[Event Loaded]=[Action hplDeleteService_Loaded($source)]"
                                                            Style="{StaticResource MainFrame.HyperlinkButton.01}"
                                                            ToolTipService.ToolTip="Xóa Dịch Vụ">
                                                            <Image
                                                                Width="13"
                                                                Height="13"
                                                                Opacity="1"
                                                                Source="/aEMR.CommonViews;component/Assets/Images/Delete.png" />
                                                        </Button>
                                                    </StackPanel>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTextColumn Binding="{Binding ObjRefMedicalServiceItem.MedServiceCode}" Header="{Binding T3364_G1_MaDV, Source={StaticResource eHCMSResources}}" />
                                        <DataGridTextColumn Binding="{Binding ObjRefMedicalServiceItem.MedServiceName}" Header="{Binding T0844_G1_TenDV, Source={StaticResource eHCMSResources}}" />
                                        <DataGridTextColumn Binding="{Binding Qty}" Header="{Binding S0583_G1_SLg, Source={StaticResource eHCMSResources}}" />
                                        <DataGridTextColumn Binding="{Binding ObjRefMedicalServiceItem.ObjV_RefMedServiceItemsUnit.ObjectValue}" Header="{Binding K3966_G1_Dvt, Source={StaticResource eHCMSResources}}" />
                                    </DataGrid.Columns>
                                </DataGrid>
                            </Grid>
                        </StackPanel>
                    </Border>
                </Grid>
            </TabItem>

            <TabItem Header="Cận lâm sàng">
                <Grid Margin="5">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition MinWidth="350" />
                        <ColumnDefinition Width="40" />
                        <ColumnDefinition MinWidth="350" />
                    </Grid.ColumnDefinitions>
                    <TextBlock
                        Grid.Row="0"
                        Margin="5,5,0,5"
                        FontSize="14"
                        FontWeight="Bold"
                        Foreground="#FF0084FF"
                        Text="Title" />
                    <Border
                        Grid.Row="0"
                        Grid.ColumnSpan="3"
                        Margin="0,10,0,5"
                        Background="AliceBlue"
                        BorderBrush="#C1D2E6"
                        BorderThickness="1"
                        CornerRadius="3">
                        <StackPanel>
                            <Border
                                Margin="10,-10,0,0"
                                HorizontalAlignment="Left"
                                Background="AliceBlue"
                                BorderBrush="#6F8AA9"
                                BorderThickness="1">
                                <TextBlock
                                    Margin="10,0,10,0"
                                    FontWeight="Bold"
                                    Text="{Binding G1174_G1_TimKiem, Source={StaticResource eHCMSResources}}" />
                            </Border>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition Width="220" />
                                    <ColumnDefinition Width="50*" />
                                    <ColumnDefinition Width="158*" />
                                    <ColumnDefinition Width="268*" />
                                    <ColumnDefinition Width="122*" />
                                </Grid.ColumnDefinitions>
                                <Label
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="0,2,5,2"
                                    HorizontalAlignment="Right"
                                    Content="{Binding K2034_G1_ChonLoai2, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}"
                                    FontWeight="Normal" />
                                <Label
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Margin="0,2,5,2"
                                    HorizontalAlignment="Right"
                                    Content="{Binding K2074_G1_ChonNhom2, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}"
                                    FontWeight="Normal" />

                                <Controls:AxComboBox
                                    x:Name="cboV_PCLMainCategory"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,2,5,2"
                                    cal:Message.Attach="[Event SelectionChanged]=[Action cboV_PCLMainCategory_SelectionChanged($this.SelectedItem)]"
                                    DisplayMemberPath="ObjectValue"
                                    FontWeight="Normal"
                                    Foreground="Black"
                                    IsEnabled="{Binding IsEnableV_PCLMainCategory, Mode=OneWay}"
                                    ItemsSource="{Binding ObjV_PCLMainCategory, Mode=OneWay}"
                                    SelectedItemEx="{Binding ObjV_PCLMainCategory_Selected, Mode=TwoWay}"
                                    SelectedValuePath="LookupID"
                                    Style="{StaticResource MainFrame.ComboBox}" />

                                <Controls:AxComboBox
                                    x:Name="cboPCLExamTypeSubCategory"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,2,5,2"
                                    cal:Message.Attach="[Event SelectionChanged]=[Action cboPCLExamTypeSubCategory_SelectionChanged($source,$eventArgs)]"
                                    DisplayMemberPath="PCLSubCategoryName"
                                    FontWeight="Normal"
                                    Foreground="Black"
                                    ItemsSource="{Binding ObjPCLExamTypeSubCategory_ByV_PCLMainCategory, Mode=OneWay}"
                                    SelectedItemEx="{Binding ObjPCLExamTypeSubCategory_ByV_PCLMainCategory_Selected, Mode=TwoWay}"
                                    SelectedValuePath="PCLExamTypeSubCategoryID"
                                    Style="{StaticResource MainFrame.ComboBox}" />

                                <Label
                                    Grid.Row="0"
                                    Grid.Column="3"
                                    Margin="0,2,5,2"
                                    HorizontalAlignment="Right"
                                    Content="Tên cận lâm sàng:"
                                    FontWeight="Normal" />
                                <TextBox
                                    x:Name="tbSearch"
                                    Grid.Row="0"
                                    Grid.Column="4"
                                    Margin="0,2,5,2"
                                    Text="{Binding SearchCriteria.PCLExamTypeName, Mode=TwoWay}" />

                                <Button
                                    x:Name="btSearchPCL"
                                    Grid.Row="1"
                                    Grid.Column="4"
                                    Width="60"
                                    Height="23"
                                    HorizontalAlignment="Left"
                                    Style="{StaticResource MainFrame.Button}"
                                    Visibility="{Binding bBtnSearch, Converter={StaticResource BooleanToVisibility}}">
                                    <StackPanel Orientation="Horizontal">
                                        <Image
                                            Width="20"
                                            Height="18"
                                            VerticalAlignment="Center"
                                            Source="/aEMR.CommonViews;component/Assets/Images/1306555029_find.png" />
                                        <TextBlock VerticalAlignment="Center" Text="{Binding G0981_G1_Tim, Source={StaticResource eHCMSResources}}" />
                                    </StackPanel>
                                </Button>
                            </Grid>
                        </StackPanel>
                    </Border>
                    <StackPanel
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="0,50,0,0"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <Button
                            x:Name="btAddChoosePCL"
                            Width="26"
                            Height="40"
                            ToolTipService.ToolTip="{Binding eHCMSResources.G0156_G1_Them, Source={StaticResource eHCMSResources}}">
                            <Image
                                Width="32"
                                Height="32"
                                Margin="-5,0,0,0"
                                HorizontalAlignment="Left"
                                Opacity="1"
                                Source="/aEMR.CommonViews;component/Assets/Images/navigate-right-icon48x48.png" />
                        </Button>
                        <Label Height="10" Content="" />
                        <Button
                            x:Name="btSavePCLItems"
                            Width="28"
                            Height="40"
                            Margin="0,0,0,10"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            ToolTipService.ToolTip="{Binding T2937_G1_Luu, Source={StaticResource eHCMSResources}}">
                            <Image Opacity="1" Source="/aEMR.CommonViews;component/Assets/Images/saveanddown24x24border.png" />
                        </Button>
                    </StackPanel>
                    <Border
                        Grid.Row="1"
                        Margin="0,10,0,5"
                        Background="AliceBlue"
                        BorderBrush="#C1D2E6"
                        BorderThickness="1"
                        CornerRadius="3">
                        <StackPanel>
                            <Border
                                Margin="10,-10,0,0"
                                HorizontalAlignment="Left"
                                Background="AliceBlue"
                                BorderBrush="#6F8AA9"
                                BorderThickness="1">
                                <TextBlock
                                    Margin="10,0,10,0"
                                    FontWeight="Bold"
                                    Text="{Binding K3014_G1_DSPCLExamType, Source={StaticResource eHCMSResources}}" />
                            </Border>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <!--<RowDefinition Height="25" />-->
                                    <RowDefinition Height="25" />
                                </Grid.RowDefinitions>
                                <Controls:ReadOnlyDataGrid
                                    Name="dtgList"
                                    Grid.Row="0"
                                    Height="275"
                                    Margin="0,5,0,0"
                                    cal:Message.Attach="[Event DblClick]=[Action PCLDoubleClick()]"
                                    AutoGenerateColumns="False"
                                    ColumnHeaderStyle="{StaticResource MainFrame.DataGridColumnHeaderStyle}"
                                    HeadersVisibility="All"
                                    HorizontalScrollBarVisibility="Auto"
                                    IsReadOnly="True"
                                    ItemsSource="{Binding ObjPCLExamTypes_List_Paging, Mode=OneWay}"
                                    SelectedItem="{Binding ObjPCLExamType_SelectForAdd, Mode=TwoWay}"
