                            MinWidth="100"
                            Margin="2,2"
                            ItemsSource="{Binding ListYear}"
                            SelectedValue="{Binding RptParameters.Year, Mode=TwoWay}"
                            Style="{StaticResource MainFrame.ComboBox2}"
