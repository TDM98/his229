                                GroupName="RegisType"
                                Style="{StaticResource MainFrame.RadioButton01}"
                                Visibility="{Binding IsVisibleKSK, Converter={StaticResource BooleanToVisibilityNot}}" />
                            <RadioButton
                                Margin="0,2,0,0"
                                cal:Message.Attach="[Event Checked] = [Action rdtKSK_Checked($source, $eventArgs)]"
                                Content="{Binding Z2670_G1_KhamSucKhoe, Source={StaticResource eHCMSResources}}"
                                GroupName="RegisType"
                                Style="{StaticResource MainFrame.RadioButton01}"
                                Visibility="{Binding IsVisibleKSK, Converter={StaticResource BooleanToVisibility}}" />
                        </StackPanel>

                        <!--  ▼==== #005 ====  -->
                        <StackPanel
                            Grid.Row="11"
                            Grid.ColumnSpan="2"
                            Margin="2,4,2,4"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Orientation="Horizontal"
                            Visibility="{Binding RptParameters.ShowFindInfectionControl, Converter={StaticResource VisibilityConverter}}">
                            <RadioButton
                                Margin="0,2,5,0"
                                cal:Message.Attach="[Event Checked] = [Action rdtICTatCa_Checked($source, $eventArgs)]"
                                Content="{Binding T0822_G1_TatCa, Source={StaticResource eHCMSResources}}"
                                GroupName="RegisType"
                                IsChecked="True"
                                Style="{StaticResource MainFrame.RadioButton01}" />
                            <RadioButton
                                Margin="0,2,5,0"
                                cal:Message.Attach="[Event Checked] = [Action rdtICMRBacteria_Checked($source, $eventArgs)]"
                                Content="VKĐK"
                                GroupName="RegisType"
                                Style="{StaticResource MainFrame.RadioButton01}" />
                            <RadioButton
                                Margin="0,2,5,0"
