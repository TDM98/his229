                            ItemsSource="{Binding InPatientDeptStatus_Idx}"
                            SelectedItem="{Binding CurrentInPatientDeptStatus, Mode=TwoWay}"
                            SelectedValuePath="Value"
                            Style="{StaticResource MainFrame.ComboBox}"
                            Visibility="{Binding mInPatientDeptStatus, Converter={StaticResource BooleanToVisibility}}" />
