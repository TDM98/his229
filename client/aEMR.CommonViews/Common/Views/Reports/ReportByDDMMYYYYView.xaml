                            Visibility="{Binding IsShowOutPtTreatmentStatus, Mode=OneWay, Converter={StaticResource BooleanToVisibility}}" />
                        <ComboBox
                            Grid.Row="9"
                            Grid.Column="1"
                            Height="24"
                            MinWidth="100"
                            Margin="2,2"
                            DisplayMemberPath="ObjectValue"
                            ItemsSource="{Binding OutPtTreatmentStatus}"
                            SelectedItem="{Binding RptParameters.V_OutPtTreatmentStatus, Mode=TwoWay}"
                            SelectedValuePath="LookupID"
                            Style="{StaticResource MainFrame.ComboBox}"
                            Visibility="{Binding IsShowOutPtTreatmentStatus, Mode=OneWay, Converter={StaticResource BooleanToVisibility}}" />
                        <StackPanel
                            Grid.Row="11"
                            Grid.ColumnSpan="2"
                            Margin="2,4,2,4"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Orientation="Horizontal"
                            Visibility="{Binding RptParameters.HideFindPatient, Converter={StaticResource VisibilityConverter}}">
                            <RadioButton
                                Margin="0,2,5,0"
                                cal:Message.Attach="[Event Checked] = [Action rdtNgoaiTru_Checked($source, $eventArgs)]"
                                Content="{Binding T3719_G1_Mau20NgTru, Source={StaticResource eHCMSResources}}"
                                GroupName="RegisType"
                                IsChecked="True"
                                Style="{StaticResource MainFrame.RadioButton01}" />
                            <RadioButton
                                Margin="0,2,5,0"
                                cal:Message.Attach="[Event Checked] = [Action rdtNoiTru_Checked($source, $eventArgs)]"
                                Content="{Binding T3713_G1_NoiTru, Source={StaticResource eHCMSResources}}"
                                GroupName="RegisType"
                                Style="{StaticResource MainFrame.RadioButton01}" />
                            <RadioButton
                                Margin="0,2,0,0"
                                cal:Message.Attach="[Event Checked] = [Action rdtTatCa_Checked($source, $eventArgs)]"
