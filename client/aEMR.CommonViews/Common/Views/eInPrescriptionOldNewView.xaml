<UserControl
    x:Class="aEMR.Common.Views.eInPrescriptionOldNewView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:AxControls="clr-namespace:aEMR.Controls;assembly=aEMR.Controls"
    xmlns:Resources="clr-namespace:eHCMSLanguage;assembly=aEMRLanguage"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:converters="clr-namespace:aEMR.Common.Converters;assembly=aEMR.Common"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:toolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="700"
    d:DesignWidth="1200"
    mc:Ignorable="d">
    <UserControl.Resources>
        <converters:StringFormatConverter x:Key="StringFormatConverter" />
        <Resources:eHCMSResources x:Key="eHCMSResources" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <converters:BooleanToVisibilityNotConverter x:Key="BooleanToVisibilityNot" />
        <converters:ColorConverter x:Key="ColorBackGroundConverter" />
        <DataTemplate x:Key="grdPrescriptionTPL_lnkDelete">
            <StackPanel
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Orientation="Horizontal">
                <Button
                    x:Name="lnkDelete"
                    cal:Message.Attach="[Event Click] = [Action lnkDelete_Click($source, $eventArgs)]"
                    Style="{StaticResource MainFrame.HyperlinkButton.01}">
                    <ToolTipService.ToolTip>
                        <TextBlock Margin="10,10,0,0" Text="{Binding G2617_G1_Xoa, Source={StaticResource eHCMSResources}}" />
                    </ToolTipService.ToolTip>
                    <Image
                        Width="13"
                        Height="13"
                        Opacity="1"
                        Source="/aEMR.CommonViews;component/Assets/Images/Delete.png" />
                </Button>
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_chk_HI">
            <CheckBox
                Name="chk_HI"
                Margin="0"
                Padding="0"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                IsChecked="{Binding BeOfHIMedicineList, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                IsEnabled="{Binding isHICheck, Mode=OneWay}"
                Style="{StaticResource MainFrame.Checkbox.Title1}" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_chk_NotInCat">
            <CheckBox
                x:Name="chk_NotInCat"
                Margin="0"
                Padding="0"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                cal:Message.Attach="[Event Click] = [Action chk_NotInCat_Click($source, $eventArgs)]"
                IsChecked="{Binding IsDrugNotInCat, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                Style="{StaticResource MainFrame.Checkbox.Title1}" />
        </DataTemplate>
        <DataTemplate x:Key="CTDrugType">
            <TextBlock
                Name="txtDrugType"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Text="{Binding DrugType.ObjectValue, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescription_DrugType">
            <AxControls:KeyEnabledComboBox
                Name="cbxDrugType"
                Margin="0,1,0,1"
                cal:Message.Attach="[Event Loaded] = [Action cbxDrugType_Loaded($source, $eventArgs)]                                    ;[Event SelectionChanged] = [Action cbxDrugType_SelectionChanged($source, $eventArgs)]"
                DisplayMemberPath="ObjectValue"
                ItemsSource="{Binding ElementName=LayoutRoot, Path=DataContext.DrugTypes}"
                SelectedItem="{Binding DrugType, Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                SelectedValuePath="LookupID"
                Style="{StaticResource MainFrame.ComboBox}" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_SelectedDrugForPrescriptionBrandName">
            <TextBlock
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Style="{StaticResource MainFrame.TextBlock.Auto}"
                Text="{Binding SelectedDrugForPrescription.BrandName, Mode=TwoWay}" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_acbDrug">
            <!--
                TxD 12/04/2014: For some unknown reason, we have to remove this Property  MinimumPrefixLength = "0"
                from the following AutoCompleteBox or Visual Studio will get an Exception when loading this XAML
                This BUG IS NOW SOLVED after a LONG LONG TIME struggling
            -->
            <AxControls:AxAutoComplete
                Name="acbDrug"
                MinWidth="300"
                HorizontalAlignment="Stretch"
                cal:Message.Attach="[Event Loaded] = [Action acbDrug_Loaded($source, $eventArgs)]                                 ;[Event Populating] = [Action acbDrug_Populating($source, $eventArgs)]                                 ;[Event DropDownClosed] = [Action acbDrug_DropDownClosed($source, $eventArgs)]                                                                 ;[Event MouseLeftButtonUp] = [Action acbDrug_MouseLeftButtonUp($source, $eventArgs)]                                        ;[Event TextChanged] = [Action acbDrug_TextChanged($source, $eventArgs)]"
                FilterMode="None"
                IsTextCompletionEnabled="False"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                ItemsSource="{Binding DrugList}"
                MinimumPopulateDelay="300"
                ScrollViewer.VerticalScrollBarVisibility="Auto"
                SelectedItem="{Binding SelectedDrugForPrescription, Mode=OneWay}"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                Text="{Binding BrandName, Mode=TwoWay}"
                ValueMemberBinding="{Binding BrandName, Mode=TwoWay}">
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Name="txtGenericName"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding GenericName}"
                                Visibility="{Binding IsSearchByGenName, Converter={StaticResource BooleanToVisibility}}" />
                            <TextBlock
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding Z0028_G1_DauNgoacTrai, StringFormat='{} {0} ', Source={StaticResource eHCMSResources}}"
                                Visibility="{Binding IsSearchByGenName, Converter={StaticResource BooleanToVisibility}}" />
                            <TextBlock
                                Name="txtDrugName"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding BrandName}" />
                            <TextBlock
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding Z0027_G1_DauNgoacPhai, StringFormat='{} {0} ', Source={StaticResource eHCMSResources}}"
                                Visibility="{Binding IsSearchByGenName, Converter={StaticResource BooleanToVisibility}}" />
                            <TextBlock
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text=" - " />
                            <TextBlock
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding S0610_G1_SLgTon, StringFormat='{}{0} :', Source={StaticResource eHCMSResources}}" />
                            <TextBlock
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding Remaining}" />
                            <TextBlock
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding A0009_G1_BH, StringFormat='{} {0}', Source={StaticResource eHCMSResources}}"
                                Visibility="{Binding InsuranceCover, Converter={StaticResource BooleanToVisibility}}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_SelectedDrugForPrescriptionContent">
            <AxControls:AxTextBox
                Background="Transparent"
                BorderBrush="Transparent"
                IsReadOnly="True"
                Text="{Binding SelectedDrugForPrescription.Content, Mode=TwoWay}"
                TextAlignment="Center" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_SelectedDrugForPrescriptionUnitName">
            <AxControls:AxTextBox
                Background="Transparent"
                BorderBrush="Transparent"
                Text="{Binding SelectedDrugForPrescription.UnitName, Mode=TwoWay}"
                TextAlignment="Center" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_dosage_Cell">
            <TextBlock
                VerticalAlignment="Center"
                Text="{Binding dosageStr}"
                TextAlignment="Right" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_dosage">
            <AxControls:AxTextBox
                x:Name="tbdosage"
                Margin="0"
                Background="Transparent"
                BorderBrush="Transparent"
                Text="{Binding dosageStr, Mode=TwoWay}"
                TextAlignment="Right" />
            <!--  IsEnabled="{Binding isHICheck, Mode=OneWay}"  -->
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_MDose_Cell">
            <TextBlock
                VerticalAlignment="Center"
                Text="{Binding MDoseStr}"
                TextAlignment="Right" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_MDose">
            <AxControls:AxTextBox
                x:Name="tbMDose"
                Margin="0"
                Background="Transparent"
                BorderBrush="Transparent"
                Text="{Binding MDoseStr, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True, UpdateSourceTrigger=PropertyChanged}"
                TextAlignment="Right" />
            <!--  IsEnabled="{Binding isHICheck, Mode=OneWay}"  -->
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_ADose_Cell">
            <TextBlock
                VerticalAlignment="Center"
                Text="{Binding ADoseStr}"
                TextAlignment="Right" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_ADose">
            <AxControls:AxTextBox
                x:Name="tbADose"
                Margin="0"
                Background="Transparent"
                BorderBrush="Transparent"
                Text="{Binding ADoseStr, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True, UpdateSourceTrigger=PropertyChanged}"
                TextAlignment="Right" />
            <!--  IsEnabled="{Binding isHICheck, Mode=OneWay}"  -->
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_EDose_Cell">
            <TextBlock
                VerticalAlignment="Center"
                Text="{Binding EDoseStr}"
                TextAlignment="Right" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_EDose">
            <AxControls:AxTextBox
                x:Name="tbEDose"
                Margin="0"
                Background="Transparent"
                BorderBrush="Transparent"
                Text="{Binding EDoseStr, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True, UpdateSourceTrigger=PropertyChanged}"
                TextAlignment="Right" />
            <!--  IsEnabled="{Binding isHICheck, Mode=OneWay}"  -->
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_NDose_Cell">
            <TextBlock
                VerticalAlignment="Center"
                Text="{Binding NDoseStr}"
                TextAlignment="Right" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_NDose">
            <AxControls:AxTextBox
                x:Name="tbNDose"
                Margin="0"
                Background="Transparent"
                BorderBrush="Transparent"
                Text="{Binding NDoseStr, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True, UpdateSourceTrigger=PropertyChanged}"
                TextAlignment="Right" />
            <!--  IsEnabled="{Binding isHICheck, Mode=OneWay}"  -->
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_DayTotal_Cell">
            <TextBlock
                VerticalAlignment="Center"
                Text="{Binding RealDay}"
                TextAlignment="Right" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_DayTotal">
            <AxControls:AxTextBox
                x:Name="tbDayTotal"
                Margin="0"
                Background="Transparent"
                BorderBrush="Transparent"
                Text="{Binding RealDay, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True, Converter={StaticResource DecimalConverter}}"
                TextAlignment="Right" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_SelectedDrugForPrescriptionUnitUse">
            <TextBlock
                VerticalAlignment="Center"
                Text="{Binding SelectedDrugForPrescription.UnitUse, Mode=TwoWay}"
                TextAlignment="Left" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_UnitUse_CellEditing">
            <!--  IsEnabled="{Binding isHICheck, Mode=OneWay}"  -->
            <AxControls:AxAutoComplete
                Name="DrugUnitUse"
                Height="auto"
                MinWidth="60"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                cal:Message.Attach="[Event Loaded] = [Action DrugUnitUse_Loaded($source, $eventArgs)]                                            ;[Event Populating] = [Action DrugUnitUse_Populating($source, $eventArgs)]                                            ;[Event SelectionChanged] = [Action DrugUnitUse_SelectionChanged($source, $eventArgs)]"
                IsDropDownOpen="True"
                IsTabStop="False"
                IsTextCompletionEnabled="False"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="0"
                ScrollViewer.VerticalScrollBarVisibility="Auto"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                Text="{Binding SelectedDrugForPrescription.UnitUse, Mode=TwoWay}"
                ValueMemberBinding="{Binding NoteDetails}">
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Name="txtDrugUnitUse"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding NoteDetails}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_SelectedDrugForPrescriptionAdministration">
            <TextBlock
                VerticalAlignment="Center"
                Text="{Binding SelectedDrugForPrescription.Administration, Mode=TwoWay}"
                TextAlignment="Left" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_Administration_CellEditing">
            <AxControls:AxAutoComplete
                Name="DrugAdministration"
                Height="Auto"
                MinWidth="200"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                cal:Message.Attach="[Event Loaded] = [Action DrugAdministration_Loaded($source, $eventArgs)]                                        ;[Event Populating] = [Action DrugAdministration_Populating($source, $eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                IsTabStop="False"
                IsTextCompletionEnabled="False"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                ItemsSource="{Binding CachDungAfterFiltering}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="1"
                ScrollViewer.VerticalScrollBarVisibility="Auto"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                Text="{Binding SelectedDrugForPrescription.Administration, Mode=TwoWay}"
                ValueMemberBinding="{Binding NoteDetails}">
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Name="txtDrugName"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding NoteDetails}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_DrugInstructionNotes_Cell">
            <TextBlock
                MaxHeight="30"
                VerticalAlignment="Center"
                ScrollViewer.VerticalScrollBarVisibility="Auto"
                Text="{Binding DrugInstructionNotes, Mode=OneWay}"
                TextAlignment="Left" />
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_DrugInstructionNotes_CellEditing">
            <AxControls:AxAutoComplete
                Name="DrugInstructionNotes"
                Height="Auto"
                MinWidth="200"
                MaxHeight="30"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                cal:Message.Attach="[Event Loaded] = [Action DrugInstructionNotes_Loaded($source, $eventArgs)]                                        ;[Event Populating] = [Action DrugInstructionNotes_Populating($source, $eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                IsTabStop="False"
                IsTextCompletionEnabled="False"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                ItemsSource="{Binding GhiChuAfterFiltering}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="1"
                ScrollViewer.VerticalScrollBarVisibility="Auto"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                Text="{Binding DrugInstructionNotes, Mode=TwoWay}"
                ValueMemberBinding="{Binding NoteDetails}">
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Name="txtDrugName"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding NoteDetails}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
    </UserControl.Resources>
    <toolkit:BusyIndicator
        Margin="0,0,0,10"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        DisplayAfter="0:0:0.100"
        IsBusy="{Binding IsProcessing, Mode=OneWay}">
        <toolkit:BusyIndicator.BusyContent>
            <TextBlock
                FontSize="14"
                FontWeight="Bold"
                Text="{Binding Path=StatusText, Mode=OneWay}" />
        </toolkit:BusyIndicator.BusyContent>
        <ContentControl
            HorizontalContentAlignment="Stretch"
            VerticalContentAlignment="Stretch"
            IsEnabled="{Binding IsEnabledForm, TargetNullValue=False, Converter={StaticResource IsEnableConverter}}">
            <Grid x:Name="LayoutRoot" Visibility="{Binding mToaThuocDaPhatHanh_ThongTin, Converter={StaticResource BooleanToVisibility}}">
                <Grid
                    HorizontalAlignment="Stretch"
                    Background="White"
                    ShowGridLines="False">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="30" />
                        <ColumnDefinition Width="*" MinWidth="100" />
                        <ColumnDefinition Width="40" />
                        <ColumnDefinition Width="40" />
                        <ColumnDefinition Width="*" MinWidth="100" />
                        <ColumnDefinition Width="5" />
                    </Grid.ColumnDefinitions>
                    <Border
                        x:Name="BorderCanvas"
                        Grid.Row="1"
                        Grid.Column="0"
                        Grid.ColumnSpan="7"
                        Width="Auto"
                        Margin="1,1,1,1"
                        Style="{StaticResource LightBlueBorderStyle}">
                        <Grid
                            Width="Auto"
                            Height="Auto"
                            ShowGridLines="False">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="5" MinWidth="5" />
                                <!--  0.Alignment  -->
                                <ColumnDefinition Width="110" MinWidth="110" />
                                <!--  1.Empty  -->
                                <ColumnDefinition Width="2*" MinWidth="50" />
                                <!--  2.Empty  -->
                                <ColumnDefinition Width="85" MinWidth="80" />
                                <!--  3.National Med code lable  -->
                                <ColumnDefinition Width="2*" MinWidth="50" />
                                <!--  4.National Med code value  -->
                                <ColumnDefinition Width="85" MinWidth="80" />
                                <!--  5.PtRecID label  -->
                                <ColumnDefinition Width="2*" MinWidth="50" />
                                <!--  6.PtRecID value  -->
                                <ColumnDefinition MinWidth="95" />
                                <!--  7.CreateDate Label  -->
                                <ColumnDefinition Width="3*" MinWidth="60" />
                                <!--  8.CreateDate Value  -->
                                <ColumnDefinition Width="5" MinWidth="5" />
                                <!--  9.Alignment  -->
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition MinHeight="1" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition MinHeight="1" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition MinHeight="1" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0" Grid.ColumnSpan="9">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.Column="0" Margin="0,0,5,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock
                                            Grid.Column="0"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            Style="{StaticResource TextBlock.Heading.04}"
                                            Text="{Binding T2222_G1_Khoa, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                                        <ContentControl
                                            Name="DepartmentContent"
                                            Grid.Column="1"
                                            MinWidth="150"
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Stretch"
                                            IsEnabled="{Binding IsEnabled}" />
                                    </Grid>
                                    <Grid Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock
                                            Grid.Column="0"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            Style="{StaticResource TextBlock.Heading.04}"
                                            Text="{Binding T2837_G1_LoaiToa, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                                        <AxControls:AxComboBox
                                            x:Name="cboV_PrescriptionType"
                                            Grid.Column="1"
                                            Width="150"
                                            Margin="0,0,5,0"
                                            DisplayMemberPath="ObjectValue"
                                            IsEnabled="{Binding IsEnabled}"
                                            ItemsSource="{Binding PrescriptionTypeList}"
                                            SelectedValueEx="{Binding LatestePrecriptions.V_PrescriptionType, Mode=TwoWay}"
                                            SelectedValuePath="LookupID"
                                            Style="{StaticResource MainFrame.ComboBox}" />
                                    </Grid>
                                    <Grid Grid.Column="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <CheckBox
                                            Grid.Column="0"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            cal:Message.Attach="[Event Checked] = [Action chkNeedToHold_Check($source, $eventArgs)];[Event Unchecked] = [Action chkNeedToHold_UnCheck($source, $eventArgs)]"
                                            Content="{Binding K1534_G1_CanHChan, Source={StaticResource eHCMSResources}}"
                                            FlowDirection="RightToLeft"
                                            IsChecked="{Binding LatestePrecriptions.NeedToHoldConsultation, Mode=TwoWay}"
                                            IsEnabled="{Binding IsEnabled}"
                                            Style="{StaticResource MainFrame.Checkbox.Title1}"
                                            TabIndex="0" />
                                        <AxControls:AxAutoComplete
                                            x:Name="aucHoldConsultDoctor"
                                            Grid.Column="1"
                                            Width="150"
                                            Margin="0,0,5,0"
                                            cal:Message.Attach="[Event Populating] = [Action aucHoldConsultDoctor_Populating($source, $eventArgs)]                                                     ;[Event SelectionChanged] = [Action aucHoldConsultDoctor_SelectionChanged($source, $eventArgs)]"
                                            FilterMode="None"
                                            IsDropDownOpen="True"
                                            IsEnabled="{Binding IsEnabledAutoComplete}"
                                            ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                                            MinimumPopulateDelay="300"
                                            MinimumPrefixLength="0"
                                            ValueMemberPath="FullName">
                                            <AxControls:AxAutoComplete.ItemTemplate>
                                                <DataTemplate>
                                                    <StackPanel>
                                                        <TextBlock
                                                            Name="txtHoldConsultDoctor"
                                                            Margin="0"
                                                            Padding="0"
                                                            Style="{StaticResource MainFrame.TextBlock.Auto}"
                                                            Text="{Binding FullName}" />
                                                    </StackPanel>
                                                </DataTemplate>
                                            </AxControls:AxAutoComplete.ItemTemplate>
                                        </AxControls:AxAutoComplete>
                                    </Grid>
                                    <ContentControl
                                        x:Name="ContentKhungSoNgayDungThuoc"
                                        Grid.Column="3"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        IsEnabled="{Binding IsEnabled}">
                                        <AxControls:AxGrid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="40" />
                                                <ColumnDefinition Width="55" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>

                                            <!--<TextBlock Text="{Binding S0674_G1_SoNDTK, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0" Margin="0,2,0,0" Style="{StaticResource TextBlock.Heading.04}"/>-->

                                            <StackPanel
                                                Grid.Row="0"
                                                Grid.Column="0"
                                                Orientation="Horizontal">
                                                <DatePicker
                                                    x:Name="dtpApptDate"
                                                    Width="110"
                                                    Height="22"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    cal:Message.Attach="[Event CalendarClosed]=[Action  ApptDate_CalendarClosed($source,$eventArgs)]"
                                                    SelectedDate="{Binding LatestePrecriptions.SelApptDate, Mode=TwoWay, NotifyOnValidationError=True}"
                                                    SelectedDateFormat="Short" />

                                                <TextBlock
                                                    Margin="0,2,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="{Binding S0674_G1_SoNDTK, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />

                                            </StackPanel>

                                            <AxControls:AxTextBox
                                                x:Name="tbSoTuan"
                                                Grid.Row="0"
                                                Grid.Column="1"
                                                Width="40"
                                                Height="20"
