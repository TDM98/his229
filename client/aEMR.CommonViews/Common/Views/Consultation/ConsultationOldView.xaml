                                Grid.Row="15"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Width="398"
                                Height="22"
                                Margin="0,2,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                cal:Message.Attach="[Event Loaded] = [Action KscV_TreatmentType_Loaded($source, $eventArgs)]; [Event SelectionChanged]=[Action cboTreatmentTypeItemChanged($source,$eventArgs)]"
                                DisplayMemberPath="ObjectValue"
                                IsEnabled="{Binding IsUpdateFromPresciption, Mode=OneWay, Converter={StaticResource BooleanNotConverter}}"
                                ItemsSource="{Binding RefTreatment}"
                                SelectedValue="{Binding DiagTrmtItem.V_TreatmentType, Mode=TwoWay}"
                                SelectedValuePath="LookupID"
                                Style="{StaticResource MainFrame.ComboBox}" />


                            <!--  Doctor comments  -->
                            <TextBlock
                                Grid.Row="16"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding K2235_G1_ChuThich2, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <TextBox
                                Name="txtDComments"
                                Grid.Row="17"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.DoctorComments, Mode=TwoWay}"
                                VerticalScrollBarVisibility="Visible" />

                            <Grid Grid.RowSpan="19" Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" MinHeight="100" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Border
                                    Grid.Row="1"
                                    Margin="2,7,2,2"
                                    Style="{StaticResource LightBlueBorderStyle}">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Border
                                            Margin="10,-12,0,0"
                                            HorizontalAlignment="Left"
                                            Style="{StaticResource LightBlueBorderStyleNo}">
                                            <TextBlock
                                                FontWeight="Bold"
                                                Foreground="#FF6F8AA9"
                                                Style="{StaticResource TextBlock.Heading.Group}"
                                                Text="{Binding Z2315_G1_KhamLamSang, Source={StaticResource eHCMSResources}}" />
                                        </Border>
                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
