                                Visibility="{Binding VisibilityOtherDiagnosis, Mode=TwoWay}" />
                            <!--  Oriented Treatment  -->
                            <TextBlock
                                Grid.Row="11"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding Z3309_G1_DienBienBenh, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <TextBox
                                Name="txtOTreatment"
                                Grid.Row="12"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.OrientedTreatment, Mode=TwoWay}"
                                VerticalScrollBarVisibility="Visible" />

                            <!--  Treatment  -->
                            <TextBlock
                                Grid.Row="14"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding K1497_G1_CachDTri2, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <TextBox
                                Grid.Row="14"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.Treatment, Mode=TwoWay}"
                                VerticalScrollBarVisibility="Visible"
                                Visibility="Collapsed" />
