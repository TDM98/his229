                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="0"
                                                Margin="0,0,5,0"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding Z2310_G1_NiemMac, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBox
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                Margin="0,2"
                                                Style="{StaticResource MainFrame.TextBox}"
                                                Text="{Binding DiagTrmtItem.KLSNiemMac, Mode=TwoWay}" />
                                            <TextBlock
                                                Grid.Row="2"
                                                Grid.Column="0"
                                                Margin="0,0,5,0"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
