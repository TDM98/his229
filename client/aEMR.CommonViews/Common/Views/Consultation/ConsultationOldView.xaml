                            <TextBox
                                Name="txtDiagnosisxd"
                                Grid.Row="1"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                IsEnabled="{Binding IsEnabled}"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.Diagnosis, Mode=TwoWay}"
                                VerticalScrollBarVisibility="Visible" />

                            <!--  Diagnosis  -->
                            <TextBlock
                                Grid.Row="3"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding K1746_G1_CDoan, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <AxControls:AxTextBoxShorthand
                                Name="txtDiagnosis"
                                Grid.Row="4"
                                Grid.RowSpan="4"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                ItemSourceEx="{Binding ShortHandDictionaryObj, Mode=OneWay}"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.DiagnosisFinal, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                VerticalScrollBarVisibility="Visible" />
                            <TextBlock
                                Grid.Row="8"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="Chẩn đoán khác"
                                TextAlignment="Center"
                                Visibility="{Binding VisibilityOtherDiagnosis, Mode=TwoWay}" />
                            <TextBox
                                Name="txtDiagnosisOther"
                                Grid.Row="9"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Height="40"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.DiagnosisOther, Mode=TwoWay}"
