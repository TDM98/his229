<UserControl
    x:Class="aEMR.Common.Views.ConsultationOldView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:AxControls="clr-namespace:aEMR.Controls;assembly=aEMR.Controls"
    xmlns:Converter="clr-namespace:aEMR.Common.Converters;assembly=aEMR.Common"
    xmlns:Resources="clr-namespace:eHCMSLanguage;assembly=aEMRLanguage"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:toolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="600"
    d:DesignWidth="1600"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Resources:eHCMSResources x:Key="eHCMSResources" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <Converter:BooleanNotConverter x:Key="BooleanNotConverter" />
        <Converter:BooleanToVisibilityNotConverter x:Key="BooleanToVisibilityNotConverter" />
        <DataTemplate x:Key="ColDeleteTemplate">
            <StackPanel
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Orientation="Horizontal">
                <Button
                    x:Name="lnkDelete"
                    cal:Message.Attach="[Event Click] = [Action lnkDelete_Click($source, $eventArgs)]"
                    ToolTipService.ToolTip="{Binding G2617_G1_Xoa, Source={StaticResource eHCMSResources}}">
                    <Image
                        Width="13"
                        Height="13"
                        Opacity="1"
                        Source="/aEMR.CommonViews;component/Assets/Images/Delete.png" />
                </Button>
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Icd10CodeDisplay">
            <TextBlock
                Name="txtDrugName"
                Margin="0"
                Padding="0"
                Style="{StaticResource MainFrame.TextBlock.01}"
                Text="{Binding DiseasesReference.ICD10Code}" />
        </DataTemplate>

        <DataTemplate x:Key="Icd10CodeAcb">
            <!--  ;[Event SelectionChanged] = [Action AutoCompleteBox_SelectionChanged($source, $eventArgs)]  -->
            <!--  Text="{Binding DiseasesReference.ICD10Code,Mode=TwoWay}" LostFocus="AxAutoComplete_LostFocus"  -->
            <AxControls:AxAutoComplete
                MinWidth="400"
                cal:Message.Attach="[Event Loaded] = [Action AcbICD10Code_Loaded($source, $eventArgs)]                                        ;[Event Populating] = [Action aucICD10_Populating($source, $eventArgs)]                                        ;[Event DropDownClosed] = [Action AxAutoComplete_DropDownClosed($source, $eventArgs)]                                        ;[Event DropDownClosing] = [Action AxAutoComplete_DropDownClosing($source, $eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="1"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                ValueMemberBinding="{Binding ICD10Code}"
                ValueMemberPath="ICD10Code">
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Name="txtDrugName"
                                Width="50"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding ICD10Code}" />
                            <TextBlock
                                Width="300"
                                Margin="5,0,0,0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding DiseaseNameVN}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
        <DataTemplate x:Key="Icd10NameDisplay">
            <StackPanel>
                <TextBlock
                    Name="txtDrugName"
                    MinWidth="150"
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.01}"
                    Text="{Binding DiseasesReference.DiseaseNameVN}" />
            </StackPanel>
        </DataTemplate>
        <!--  Text="{Binding DiseasesReference.DiseaseNameVN}"  -->
        <!--  SelectedItem="{Binding DiseasesReference, Mode=TwoWay}"  -->
        <DataTemplate x:Key="Icd10NameAcb">
            <AxControls:AxAutoComplete
                Name="DiseaseName"
                Grid.Row="0"
                Grid.Column="1"
                cal:Message.Attach="[Event Loaded] = [Action AcbICD10Name_Loaded($source, $eventArgs)]                                         ;[Event Populating] = [Action aucICD10Name_Populating($source, $eventArgs)]                                         ;[Event DropDownClosed] = [Action DiseaseName_DropDownClosed($source, $eventArgs)]                                         ;[Event DropDownClosing] = [Action DiseaseName_DropDownClosing($source, $eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="0"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                Text="{Binding DiseasesReference.DiseaseNameVN}"
                ValueMemberBinding="{Binding DiseaseNameVN, Mode=TwoWay}"
                ValueMemberPath="DiseaseNameVN">
                <!--    -->
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel>
                            <TextBlock
                                Name="txtDrugName"
                                MinWidth="150"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding DiseaseNameVN}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
        <DataTemplate x:Key="LoadEditStatus">
            <AxControls:KeyEnabledComboBox
                Name="cbxChooseDose"
                Margin="0,1,0,1"
                cal:Message.Attach="[Event Loaded] = [Action UserControl_Loaded($source, $eventArgs)]"
                Background="Transparent"
                BorderBrush="Transparent"
                DisplayMemberPath="ObjectValue"
                SelectedItem="{Binding LookupStatus, Mode=TwoWay}"
                SelectedValuePath="LookupID"
                Style="{StaticResource MainFrame.ComboBox}" />
        </DataTemplate>
        <DataTemplate x:Key="LoadStatus">
            <TextBlock Text="{Binding LookupStatus.ObjectValue}" />
        </DataTemplate>
    </UserControl.Resources>

    <toolkit:BusyIndicator
        Margin="0,0,0,10"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        DisplayAfter="0:0:0.100"
        IsBusy="{Binding IsProcessing, Mode=OneWay}">
        <toolkit:BusyIndicator.BusyContent>
            <TextBlock
                FontSize="14"
                FontWeight="Bold"
                Text="{Binding Path=StatusText, Mode=OneWay}" />
        </toolkit:BusyIndicator.BusyContent>

        <Grid x:Name="LayoutRoot" Background="White">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <ContentControl
                Grid.Row="0"
                HorizontalContentAlignment="Stretch"
                VerticalContentAlignment="Stretch"
                IsEnabled="{Binding FormEditorIsEnabled, Mode=OneWay}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <!--  PMR Info  -->
                    <Border
                        x:Name="BorderCanvas"
                        Grid.Row="0"
                        Grid.Column="0"
                        Margin="1,1,1,1"
                        Style="{StaticResource LightBlueBorderStyle}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding N0082_G1_NgKham, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <TextBlock
                                    x:Name="txtExamDate"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding DiagTrmtItem.PatientServiceRecord.ExamDate, StringFormat='dd/MM/yyyy HH:mm:ss', Mode=TwoWay}"
                                    TextWrapping="NoWrap" />
                                <CheckBox
                                    Grid.Column="2"
                                    Grid.ColumnSpan="2"
                                    Margin="60,0,3,0"
                                    HorizontalAlignment="Left"
                                    Content="{Binding Z1328_G1_SuyTim, StringFormat=':{0}', Source={StaticResource eHCMSResources}}"
                                    FlowDirection="RightToLeft"
                                    IsChecked="{Binding DiagTrmtItem.HeartFailureType, Mode=TwoWay}" />
                                <!--<TextBlock Visibility="{Binding VisibilyDiagnosisType}" Text="{Binding T2692_G1_LoaiCDoanoan, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Column="2" Style="{StaticResource MainFrame.TextBlock.01}" />-->
                                <!--<ComboBox Visibility="{Binding VisibilyDiagnosisType}"  Grid.Column="3" VerticalAlignment="Center"
                              ItemsSource="{Binding RefDiagnosis}"
                              DisplayMemberPath="ObjectValue" SelectedValuePath="LookupID"
                              SelectedValue="{Binding DiagTrmtItem.PatientServiceRecord.V_DiagnosisType, Mode=TwoWay}"
                              Style="{StaticResource MainFrame.ComboBox}" />-->
                            </Grid>

                            <Line
                                Grid.Row="1"
                                Grid.Column="1"
                                Grid.ColumnSpan="10"
                                Height="1"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Top"
                                Fill="#FFF4F4F5"
                                Stroke="#FFCCCCCC" />

                            <Grid Grid.Row="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>

                                <TextBlock
                                    Grid.Column="0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding G0936_G1_TieuDe, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <ComboBox
                                    Name="cboVBehaving"
                                    Grid.Column="1"
                                    VerticalAlignment="Center"
                                    DisplayMemberPath="ObjectValue"
                                    ItemsSource="{Binding RefBehaving}"
                                    SelectedValue="{Binding DiagTrmtItem.PatientServiceRecord.V_Behaving, Mode=TwoWay}"
                                    SelectedValuePath="LookupID"
                                    Style="{StaticResource MainFrame.ComboBox}" />

                                <TextBlock
                                    Grid.Column="2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding T3742_G1_MauBAn, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <ComboBox
                                    Name="cboPMRTemplate"
                                    Grid.Column="3"
                                    VerticalAlignment="Center"
                                    DisplayMemberPath="TemplateName"
                                    ItemsSource="{Binding RefMedRecTemplate}"
                                    SelectedValue="{Binding DiagTrmtItem.MDRptTemplateID, Mode=TwoWay}"
                                    SelectedValuePath="MDRptTemplateID"
                                    Style="{StaticResource MainFrame.ComboBox}" />

                            </Grid>
                        </Grid>
                    </Border>
                    <Border
