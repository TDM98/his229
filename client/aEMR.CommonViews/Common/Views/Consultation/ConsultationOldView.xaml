                                            <TextBox
                                                Grid.Row="2"
                                                Grid.Column="1"
                                                Margin="0,2"
                                                Style="{StaticResource MainFrame.TextBox}"
                                                Text="{Binding DiagTrmtItem.KLSKetMac, Mode=TwoWay}" />
                                            <TextBlock
                                                Grid.Row="3"
                                                Grid.Column="0"
                                                Margin="0,0,5,0"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding A1176_G1_TuyenGiap, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBox
                                                Grid.Row="3"
                                                Grid.Column="1"
                                                Margin="0,2"
                                                Style="{StaticResource MainFrame.TextBox}"
                                                Text="{Binding DiagTrmtItem.KLSTuyenGiap, Mode=TwoWay}" />
                                            <TextBlock
                                                Grid.Row="4"
                                                Grid.Column="0"
                                                Margin="0,0,5,0"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding Z2311_G1_HachBachHuyet, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBox
                                                Grid.Row="4"
                                                Grid.Column="1"
                                                Margin="0,2"
                                                Style="{StaticResource MainFrame.TextBox}"
                                                Text="{Binding DiagTrmtItem.KLSHachBachHuyet, Mode=TwoWay}" />
                                            <TextBlock
                                                Grid.Row="5"
