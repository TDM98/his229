<UserControl
    x:Class="aEMR.Common.Views.ConsultationOld_V3View"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:AxControls="clr-namespace:aEMR.Controls;assembly=aEMR.Controls"
    xmlns:Resources="clr-namespace:eHCMSLanguage;assembly=aEMRLanguage"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:toolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="600"
    d:DesignWidth="2000"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Resources:eHCMSResources x:Key="eHCMSResources" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <DataTemplate x:Key="ColDeleteTemplate">
            <StackPanel
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Orientation="Horizontal">
                <Button
                    x:Name="lnkDelete"
                    cal:Message.Attach="[Event Click]=[Action lnkDelete_Click($source,$eventArgs)]"
                    ToolTipService.ToolTip="{Binding G2617_G1_Xoa, Source={StaticResource eHCMSResources}}">
                    <Image
                        Width="13"
                        Height="13"
                        Opacity="1"
                        Source="/aEMR.CommonViews;component/Assets/Images/Delete.png" />
                </Button>
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Icd10CodeDisplay">
            <TextBlock
                Name="txtDrugName"
                Margin="0"
                Padding="0"
                Style="{StaticResource MainFrame.TextBlock.01}"
                Text="{Binding DiseasesReference.ICD10Code}" />
        </DataTemplate>

        <DataTemplate x:Key="Icd10CodeAcb">
            <!--  ;[Event SelectionChanged]=[Action AutoCompleteBox_SelectionChanged($source,$eventArgs)]  -->
            <!--  Text="{Binding DiseasesReference.ICD10Code,Mode=TwoWay}" LostFocus="AxAutoComplete_LostFocus"  -->
            <AxControls:AxAutoComplete
                MinWidth="400"
                cal:Message.Attach="[Event Loaded]=[Action AcbICD10Code_Loaded($source,$eventArgs)]                                        ;[Event Populating]=[Action aucICD10_Populating($source,$eventArgs)]                                        ;[Event DropDownClosed]=[Action AxAutoComplete_DropDownClosed($source,$eventArgs)]                                        ;[Event DropDownClosing]=[Action AxAutoComplete_DropDownClosing($source,$eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                ItemContainerStyle="{DynamicResource ResourceKey=CusLBStyle}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="1"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                ValueMemberBinding="{Binding ICD10Code}"
                ValueMemberPath="ICD10Code">
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Name="txtDrugName"
                                Width="75"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding ICD10Code}" />
                            <TextBlock
                                Width="300"
                                Margin="5,0,0,0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding DiseaseNameVN}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
        <DataTemplate x:Key="Icd10NameDisplay">
            <StackPanel>
                <TextBlock
                    Name="txtDrugName"
                    MinWidth="150"
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.01}"
                    Text="{Binding DiseasesReference.DiseaseNameVN}" />
            </StackPanel>
        </DataTemplate>
        <!--  Text="{Binding DiseasesReference.DiseaseNameVN}"  -->
        <!--  SelectedItem="{Binding DiseasesReference,Mode=TwoWay}"  -->
        <DataTemplate x:Key="Icd10NameAcb">
            <AxControls:AxAutoComplete
                Name="DiseaseName"
                Grid.Row="0"
                Grid.Column="1"
                cal:Message.Attach="[Event Loaded]=[Action AcbICD10Name_Loaded($source,$eventArgs)]                                         ;[Event Populating]=[Action aucICD10Name_Populating($source,$eventArgs)]                                         ;[Event DropDownClosed]=[Action DiseaseName_DropDownClosed($source,$eventArgs)]                                         ;[Event DropDownClosing]=[Action DiseaseName_DropDownClosing($source,$eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                ItemContainerStyle="{DynamicResource ResourceKey=CusLBStyle}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="0"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                Text="{Binding DiseasesReference.DiseaseNameVN}"
                ValueMemberBinding="{Binding DiseaseNameVN, Mode=TwoWay}"
                ValueMemberPath="DiseaseNameVN">
                <!--    -->
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel>
                            <TextBlock
                                Name="txtDrugName"
                                MinWidth="150"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding DiseaseNameVN}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
        <DataTemplate x:Key="LoadEditStatus">
            <AxControls:KeyEnabledComboBox
                Name="cbxChooseDose"
                Margin="0,1,0,1"
                cal:Message.Attach="[Event Loaded]=[Action UserControl_Loaded($source,$eventArgs)]"
                Background="Transparent"
                BorderBrush="Transparent"
                DisplayMemberPath="ObjectValue"
                SelectedItem="{Binding LookupStatus, Mode=TwoWay}"
                SelectedValuePath="LookupID"
                Style="{StaticResource MainFrame.ComboBox}" />
        </DataTemplate>
        <DataTemplate x:Key="LoadStatus">
            <TextBlock Text="{Binding LookupStatus.ObjectValue}" />
        </DataTemplate>
        <Style x:Key="CusLBStyle" TargetType="ListBoxItem">
            <Setter Property="FontSize" Value="22" />
            <Style.Triggers>
                <DataTrigger Binding="{Binding Path=IsNewInYear}" Value="True">
                    <Setter Property="Background" Value="#95dcfc" />
                </DataTrigger>
            </Style.Triggers>
        </Style>
    </UserControl.Resources>

    <toolkit:BusyIndicator
        Margin="0,0,0,10"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        DisplayAfter="0:0:0.100"
        IsBusy="{Binding IsProcessing, Mode=OneWay}">
        <toolkit:BusyIndicator.BusyContent>
            <TextBlock
                FontSize="14"
                FontWeight="Bold"
                Text="{Binding Path=StatusText, Mode=OneWay}" />
        </toolkit:BusyIndicator.BusyContent>

        <Grid x:Name="LayoutRoot" Background="White">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <ContentControl
                Grid.Row="0"
                HorizontalContentAlignment="Stretch"
                VerticalContentAlignment="Stretch">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <!--  PMR Info  -->
                    <Border
                        x:Name="BorderCanvas"
                        Grid.Row="0"
                        Grid.Column="0"
                        Margin="1,1,1,1"
                        Style="{StaticResource LightBlueBorderStyle}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding N0082_G1_NgKham, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                                <TextBlock
                                    x:Name="txtExamDate"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding DiagTrmtItem.DiagnosisDate, StringFormat='dd/MM/yyyy HH:mm:ss', Mode=TwoWay}"
                                    TextWrapping="NoWrap" />

                                <TextBlock
                                    Grid.Column="1"
                                    Margin="160,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="Giờ đăng ký:" />
                                <TextBlock
                                    Grid.Column="1"
                                    Margin="230,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding Registration_DataStorage.CurrentPatientRegistration.ExamDate, StringFormat='dd/MM/yyyy HH:mm:ss', Mode=TwoWay}" />
                                <StackPanel
                                    Grid.Column="2"
                                    Grid.ColumnSpan="2"
                                    Orientation="Horizontal">
                                    <CheckBox
                                        Margin="60,0,3,0"
                                        HorizontalAlignment="Left"
                                        Content="{Binding Z1328_G1_SuyTim, StringFormat=':{0}', Source={StaticResource eHCMSResources}}"
                                        FlowDirection="RightToLeft"
                                        IsChecked="{Binding DiagTrmtItem.HeartFailureType, Mode=TwoWay}"
                                        IsEnabled="{Binding FormEditorIsEnabled, Mode=OneWay}" />
                                    <CheckBox
                                        x:Name="IsInTreatmentProgramCheckBox"
                                        Margin="60,0,3,0"
                                        HorizontalAlignment="Left"
                                        cal:Message.Attach="[Event Checked]=[Action IsInTreatmentProgramCheckBox_CheckedChanged($source,$eventArgs)]                                               ;[Event Unchecked]=[Action IsInTreatmentProgramCheckBox_CheckedChanged($source,$eventArgs)]"
                                        Content="{Binding Z2949_G1_DieuTriNgoaiTru, StringFormat=':{0}', Source={StaticResource eHCMSResources}}"
                                        FlowDirection="RightToLeft"
                                        IsChecked="{Binding Registration_DataStorage.CurrentPatientRegistration.IsInTreatmentProgram, Mode=OneWay}"
                                        IsEnabled="False"
                                        Style="{StaticResource MainFrame.Checkbox.MiddleTitle}"
                                        Visibility="{Binding IsOutPtTreatmentProgram, Converter={StaticResource BooleanToVisibility}}" />
                                    <Button
                                        x:Name="TreatmentProgramListButton"
                                        Margin="60,0,3,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Content="{Binding Z2954_G1_DanhSachDieuTri, Source={StaticResource eHCMSResources}}"
                                        IsEnabled="{Binding IsHasOldRegistration}"
                                        Style="{StaticResource MainFrame.HyperlinkButton.01}"
                                        Visibility="{Binding IsOutPtTreatmentProgram, Converter={StaticResource BooleanToVisibility}}" />
                                </StackPanel>
                                <!--<TextBlock Visibility="{Binding VisibilyDiagnosisType}" Text="{Binding T2692_G1_LoaiCDoanoan,StringFormat='{}{0}: ',Source={StaticResource eHCMSResources}}" HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Column="2" Style="{StaticResource MainFrame.TextBlock.01}" />-->
                                <!--<ComboBox Visibility="{Binding VisibilyDiagnosisType}" Grid.Column="3" VerticalAlignment="Center"
                              ItemsSource="{Binding RefDiagnosis}"
                              DisplayMemberPath="ObjectValue" SelectedValuePath="LookupID"
                              SelectedValue="{Binding DiagTrmtItem.PatientServiceRecord.V_DiagnosisType,Mode=TwoWay}"
                              Style="{StaticResource MainFrame.ComboBox}" />-->
                            </Grid>
                            <StackPanel
                                Grid.Row="1"
                                IsEnabled="{Binding FormEditorIsEnabled, Mode=OneWay}"
                                Orientation="Horizontal">
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Margin="54,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding Z2863_G1_GioBatDau, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding DateStarted, StringFormat='dd/MM/yyyy HH:mm:ss', Mode=TwoWay}" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Margin="55,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding Z2864_G1_GioKetThuc, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding DateEnded, StringFormat='dd/MM/yyyy HH:mm:ss', Mode=TwoWay}" />
                            </StackPanel>
                            <Line
                                Grid.Row="1"
                                Grid.Column="1"
                                Grid.ColumnSpan="10"
                                Height="1"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Top"
                                Fill="#FFF4F4F5"
                                Stroke="#FFCCCCCC" />
                            <Grid Grid.Row="2" IsEnabled="{Binding FormEditorIsEnabled, Mode=OneWay}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Column="0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding G0936_G1_TieuDe, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}"
                                    Visibility="Collapsed" />
                                <ComboBox
                                    Name="cboVBehaving"
                                    Grid.Column="1"
                                    VerticalAlignment="Center"
                                    DisplayMemberPath="ObjectValue"
                                    ItemsSource="{Binding RefBehaving}"
                                    SelectedValue="{Binding DiagTrmtItem.PatientServiceRecord.V_Behaving, Mode=TwoWay}"
                                    SelectedValuePath="LookupID"
                                    Style="{StaticResource MainFrame.ComboBox}"
                                    Visibility="Collapsed" />
                                <TextBlock
                                    Grid.Column="2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding T3742_G1_MauBAn, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}"
                                    Visibility="Collapsed" />
                                <ComboBox
                                    Name="cboPMRTemplate"
                                    Grid.Column="3"
                                    VerticalAlignment="Center"
                                    DisplayMemberPath="TemplateName"
                                    ItemsSource="{Binding RefMedRecTemplate}"
                                    SelectedValue="{Binding DiagTrmtItem.MDRptTemplateID, Mode=TwoWay}"
                                    SelectedValuePath="MDRptTemplateID"
                                    Style="{StaticResource MainFrame.ComboBox}"
                                    Visibility="Collapsed" />
                            </Grid>
                        </Grid>
                    </Border>
                    <Border
                        Grid.Row="0"
                        Grid.Column="1"
                        Margin="1,1,1,1"
                        IsEnabled="{Binding FormEditorIsEnabled, Mode=OneWay}"
                        Style="{StaticResource LightBlueBorderStyle}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding K1373_G1_BSiCDoan, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="4"
                                Margin="2,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding DiagTrmtItem.ObjDoctorStaffID.FullName}"
                                TextWrapping="NoWrap" />
                            <TextBlock
                                Grid.Row="1"
                                Grid.Column="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding K3449_G1_DienThoai, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                            <TextBlock
                                Grid.Row="1"
                                Grid.Column="4"
                                Margin="2,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding DiagTrmtItem.ObjDoctorStaffID.PhoneNumberString}"
                                TextWrapping="NoWrap" />
                        </Grid>
                    </Border>
                    <Border
                        Grid.Row="1"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        Margin="2,1"
                        IsEnabled="{Binding FormEditorIsEnabled, Mode=OneWay}"
                        Style="{StaticResource LightBlueBorderStyle}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50*" />
                                <ColumnDefinition Width="50*" />
                            </Grid.ColumnDefinitions>
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Orientation="Horizontal">
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Grid.ColumnSpan="2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding Z0020_G1_TrieuChungDauHieuLamSang, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                                <TextBlock
                                    Grid.Row="14"
                                    Margin="2,2,1,2"
                                    VerticalAlignment="Top"
                                    Style="{StaticResource TBlock.Heading.03.RedAsterisk}"
                                    Text="{Binding Z0045_G1_DauSao, Source={StaticResource eHCMSResources}}"
                                    Visibility="{Binding Registration_DataStorage.CurrentPatientRegistration.IsInTreatmentProgram, Mode=OneWay, Converter={StaticResource BooleanToVisibility}}" />
                            </StackPanel>
                            <TextBox
                                Name="txtDiagnosisxd"
                                Grid.Row="1"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                IsEnabled="{Binding IsEnabled}"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.Diagnosis, Mode=TwoWay}"
                                VerticalScrollBarVisibility="Visible" />

                            <!--  Diagnosis  -->
                            <TextBlock
                                Grid.Row="3"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding K1746_G1_CDoan, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                            <AxControls:AxTextBoxShorthand
                                Name="txtDiagnosis"
                                Grid.Row="4"
                                Grid.RowSpan="4"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                IsEnabled="{Binding DiagnosisFinalIsEnabled, Mode=OneWay}"
                                ItemSourceEx="{Binding ShortHandDictionaryObj, Mode=OneWay}"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.DiagnosisFinal, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                VerticalScrollBarVisibility="Visible" />
                            <StackPanel
                                Grid.Row="14"
                                Grid.Column="1"
                                Grid.ColumnSpan="2"
                                Orientation="Horizontal">
                                <TextBlock
                                    Grid.Row="14"
                                    Grid.Column="0"
                                    Grid.ColumnSpan="2"
                                    Height="20"
                                    Margin="0,2,3,2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding Z3310_G1_GiaiDoanBenh, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}"
                                    TextAlignment="Center" />
                            </StackPanel>

                            <TextBox
                                Grid.Row="15"
                                Grid.RowSpan="2"
                                Grid.Column="1"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.DiseaseStage, Mode=TwoWay}"
                                VerticalScrollBarVisibility="Visible" />
                            <TextBlock
                                Grid.Row="8"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="Chẩn đoán khác"
                                TextAlignment="Center"
                                Visibility="{Binding VisibilityOtherDiagnosis, Mode=TwoWay}" />
                            <TextBox
                                Name="txtDiagnosisOther"
                                Grid.Row="9"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Height="40"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.DiagnosisOther, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                Visibility="{Binding VisibilityOtherDiagnosis, Mode=TwoWay}" />
                            <!--  Oriented Treatment  -->
                            <StackPanel
                                Grid.Row="11"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Orientation="Horizontal">
                                <TextBlock
                                    Grid.Row="11"
                                    Grid.Column="0"
                                    Grid.ColumnSpan="2"
                                    Height="20"
                                    Margin="0,2,3,2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding Z3309_G1_DienBienBenh, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}"
                                    TextAlignment="Center" />
                                <TextBlock
                                    Grid.Row="14"
                                    Margin="2,2,1,2"
                                    VerticalAlignment="Top"
                                    Style="{StaticResource TBlock.Heading.03.RedAsterisk}"
                                    Text="{Binding Z0045_G1_DauSao, Source={StaticResource eHCMSResources}}" />
                                <Button
                                    x:Name="btDiseaseProgression"
                                    Width="20"
                                    Height="15"
                                    Margin="2,0,1,2"
                                    VerticalAlignment="Top"
                                    VerticalContentAlignment="Top"
                                    Content="..." />
                            </StackPanel>
                            <TextBox
                                Name="txtOTreatment"
                                Grid.Row="12"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Height="40"
                                Margin="0,-10,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.OrientedTreatment, Mode=TwoWay}"
                                VerticalScrollBarVisibility="Visible" />
                            <!--  Treatment  -->
                            <TextBox
                                Grid.Row="12"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.Treatment, Mode=TwoWay}"
                                VerticalScrollBarVisibility="Visible"
                                Visibility="Collapsed" />
                            <!--  Doctor comments  -->
                            <StackPanel
                                Grid.Row="14"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Orientation="Horizontal">
                                <TextBlock
                                    Grid.Row="14"
                                    Grid.Column="0"
                                    Grid.ColumnSpan="2"
                                    Height="20"
                                    Margin="0,2,3,2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding Z3200_G1_LyDoVaoNgT, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}"
                                    TextAlignment="Center" />
                                <TextBlock
                                    Grid.Row="14"
                                    Margin="2,2,1,2"
                                    VerticalAlignment="Top"
                                    Style="{StaticResource TBlock.Heading.03.RedAsterisk}"
                                    Text="{Binding Z0045_G1_DauSao, Source={StaticResource eHCMSResources}}" />
                            </StackPanel>

                            <TextBox
                                Grid.Row="15"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.ReasonHospitalStay, Mode=TwoWay}"
                                VerticalScrollBarVisibility="Visible" />

                            <Border
                                Grid.Row="0"
                                Grid.RowSpan="7"
                                Grid.Column="1"
                                Grid.ColumnSpan="2"
                                Margin="2,7,2,0"
                                Style="{StaticResource LightBlueBorderStyle}">
                                <Grid>
                                    <Grid.RowDefinitions>
