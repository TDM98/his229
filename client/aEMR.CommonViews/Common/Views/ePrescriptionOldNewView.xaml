        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_NDose_Cell">
            <TextBlock Text="{Binding NDoseStr}" TextAlignment="Right" VerticalAlignment="Center"/>
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_NDose">
            <AxControls:AxTextBox x:Name="tbNDose" Margin="0" Text="{Binding NDoseStr,Mode=TwoWay,NotifyOnValidationError=True,ValidatesOnExceptions=True,UpdateSourceTrigger=PropertyChanged}" TextAlignment="Right" BorderBrush="Transparent" Background="Transparent"
                    />-->
