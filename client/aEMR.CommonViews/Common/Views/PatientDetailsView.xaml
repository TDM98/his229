<customcontrols:AxUserControl
    x:Class="aEMR.Common.Views.PatientDetailsView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Resources="clr-namespace:eHCMSLanguage;assembly=aEMRLanguage"
    xmlns:aEMRCommConv="clr-namespace:aEMR.Common.Converters;assembly=aEMR.Common"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:customcontrols="clr-namespace:aEMR.Controls;assembly=aEMR.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="600"
    d:DesignWidth="1500"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Resources:eHCMSResources x:Key="eHCMSResources" />
        <aEMRCommConv:EnumConverter x:Key="EnumConverterObj" />
        <aEMRCommConv:EnumVisibilityConverter x:Key="EnumVisibilityConverter" />
        <aEMRCommConv:VisibilityConverter x:Key="VisibilityConverter" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <aEMRCommConv:BooleanToVisibilityNotConverter x:Key="BooleanToNotVisibility" />
    </UserControl.Resources>
    <!--<customcontrols:BusyIndicator HorizontalAlignment="Stretch"
                                         VerticalAlignment="Stretch"
                                         IsBusy="{Binding IsProcessing, Mode=OneWay}"
                                         DisplayAfter="0:0:0.100"
                                         Margin="0,0,0,10">-->
    <xctk:BusyIndicator BusyContent="{Binding DlgBusyContent}" IsBusy="{Binding DlgIsBusyFlag}">
        <Grid
            x:Name="LayoutRoot"
            MaxWidth="940"
            MaxHeight="650"
            Margin="0"
            ScrollViewer.HorizontalScrollBarVisibility="Auto"
            ScrollViewer.VerticalScrollBarVisibility="Auto">
            <Grid.RowDefinitions>
                <RowDefinition Height="25" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="24" />
                <RowDefinition Height="30" />
            </Grid.RowDefinitions>
            <TextBlock
                x:Name="TitleForm"
                Grid.ColumnSpan="2"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Style="{StaticResource MainFrame.TextBlock.Header.01}"
                Text="{Binding CurrentAction}" />
            <StackPanel
                x:Name="panelContent"
                Grid.Row="1"
                Margin="0,0,0,0"
                VerticalAlignment="Top">
                <!--  Style="{StaticResource MainFrame.TabControl}"  -->
                <TabControl
                    x:Name="tabPatientInfo"
                    Margin="0"
                    VerticalAlignment="Top"
                    cal:Message.Attach="[Event SelectionChanged]=[Action PatientTabsChanged($source, $eventArgs)]"
                    IsTabStop="False"
                    Style="{StaticResource MainFrame.TabControl}">
                    <TabItem
                        x:Name="tabitemGeneral"
                        Header="{Binding G0574_G1_TTinChung, Source={StaticResource eHCMSResources}}"
                        Visibility="{Binding Path=ActivationMode, ConverterParameter=CREATE_PATIENT|NEW_PATIENT_FOR_NEW_REGISTRATION|CREATE_NEW_PATIENT_DETAILS_ONLY|VIEW_AND_EDIT_PATIENT|EDIT_PATIENT_FOR_REGISTRATION|EDIT_PATIENT_GENERAL_INFO|PATIENT_GENERAL_HI_VIEW, Mode=OneWay, Converter={StaticResource EnumVisibilityConverter}}">
                        <Grid>
                            <ScrollViewer
                                Padding="0"
                                BorderThickness="0"
                                HorizontalScrollBarVisibility="Auto"
                                VerticalScrollBarVisibility="Auto">
                                <Grid x:Name="gridGeneralInfo">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition Width="20" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <StackPanel Width="440">
                                        <Border
                                            x:Name="groupGeneralInfo"
                                            Margin="0,0,0,0"
                                            Style="{StaticResource LightBlueBorderStyleNew}">
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="0"
                                                    Foreground="#FF5E5E5E"
                                                    Style="{StaticResource TBlock.Heading.02}"
                                                    Text="{Binding G0574_G1_TTinChung, Source={StaticResource eHCMSResources}}"
                                                    TextWrapping="Wrap" />
                                                <Grid Width="430" Margin="0,0,0,0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                        <RowDefinition Height="24" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="10" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock
                                                        Grid.Row="0"
                                                        Grid.Column="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding N0111_G1_NgTao, Source={StaticResource eHCMSResources}}" />

                                                    <customcontrols:AxDateTextBox
                                                        x:Name="txtDateBecamePatient"
                                                        Grid.Column="1"
                                                        Height="24"
                                                        VerticalAlignment="Top"
                                                        VerticalContentAlignment="Center"
                                                        cal:Message.Attach="[Event TextChanged] = [Action txtDateBecamePatient_TextChanged($source,$eventArgs)]"
                                                        Date="{Binding CurrentPatient.DateBecamePatient, Mode=OneWay, StringFormat=dd/MM/yyyy, ConverterCulture=vi-VN}"
                                                        IsEnabled="{Binding Path=FormState, ConverterParameter=NEW|EDIT, Mode=OneWay, Converter={StaticResource EnumConverterObj}}" />

                                                    <!--<maskedInput:MaskedEdit Mask="LongDate" x:Name="txtDateBecamePatient"
                                                                            IsEnabled="{Binding Path=FormState, ConverterParameter=NEW|EDIT, Mode=OneWay, Converter={StaticResource EnumConverterObj}}"
                                                                            Text="{Binding CurrentPatient.DateBecamePatient, Mode=TwoWay, ConverterCulture=vi-VN}" Grid.Column="1"
                                                                             TabIndex="30" Height="24" VerticalAlignment="Top" />-->

                                                    <TextBlock
                                                        Grid.Row="0"
                                                        Grid.Column="4"
                                                        Margin="10,0,0,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding T3220_G1_MaBN, StringFormat='{}{0}: ', Source={StaticResource eHCMSResources}}" />
                                                    <TextBox
                                                        Grid.Row="0"
                                                        Grid.Column="5"
                                                        VerticalContentAlignment="Center"
                                                        IsReadOnly="True"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        Text="{Binding CurrentPatient.PatientCode, Mode=TwoWay}" />
                                                    <TextBlock
                                                        Grid.Row="1"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding T1567_G1_HoTen, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <!--  Style="{StaticResource MainFrame.TextBox}"  -->
                                                    <TextBox
                                                        x:Name="txtFullName"
                                                        Grid.Row="1"
                                                        Grid.Column="1"
                                                        Grid.ColumnSpan="5"
                                                        Height="22"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        VerticalContentAlignment="Center"
                                                        IsEnabled="{Binding IsEnableEdit, Mode=OneWay}"
                                                        IsReadOnly="{Binding Path=FormState, ConverterParameter=NONE|READONLY, Mode=OneWay, Converter={StaticResource EnumConverterObj}}"
                                                        IsTabStop="True"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        Text="{Binding CurrentPatient.FullName, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                    <TextBlock
                                                        Grid.Row="1"
                                                        Grid.Column="6"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TBlock.Heading.03.RedAsterisk}"
                                                        Text="{Binding Z0045_G1_DauSao, Source={StaticResource eHCMSResources}}" />

                                                    <TextBlock
                                                        Grid.Row="2"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding N0036_G1_NSinh, Source={StaticResource eHCMSResources}}" />
                                                    <TextBlock
                                                        Grid.Row="2"
                                                        Grid.Column="6"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TBlock.Heading.03.RedAsterisk}"
                                                        Text="{Binding Z0045_G1_DauSao, Source={StaticResource eHCMSResources}}" />

                                                    <StackPanel
                                                        Grid.Row="2"
                                                        Grid.Column="1"
                                                        Grid.ColumnSpan="4"
                                                        HorizontalAlignment="Stretch"
                                                        Orientation="Horizontal">
                                                        <customcontrols:AxTextBoxFilter
                                                            x:Name="txtYOB"
                                                            Width="40"
                                                            Height="22"
                                                            Margin="0"
                                                            VerticalAlignment="Center"
                                                            cal:Message.Attach="[Event LostFocus] = [Action txtYOB_LostFocus($source,$eventArgs)];[Event KeyUp] = [Action txtYOB_KeyUp($source,$eventArgs)]"
                                                            Filter="{Binding IntNumberFilter, Mode=OneWay}"
                                                            IsReadOnly="{Binding Path=FormState, ConverterParameter=NONE|READONLY, Mode=OneWay, Converter={StaticResource EnumConverterObj}}"
                                                            Style="{StaticResource MainFrame.TextBox}"
                                                            Text="{Binding CurrentPatient.YOB, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnDataErrors=True}" />
                                                        <TextBlock
                                                            Margin="5,0,5,0"
                                                            VerticalAlignment="Center"
                                                            Style="{StaticResource TextBlock.Heading.04}"
                                                            Text="{Binding G2057_G1_Tuoi, Source={StaticResource eHCMSResources}}"
                                                            TextWrapping="Wrap" />
                                                        <customcontrols:AxTextBoxFilter
                                                            x:Name="txtAge"
                                                            Width="30"
                                                            Height="22"
                                                            Margin="0"
                                                            VerticalAlignment="Center"
                                                            cal:Message.Attach="[Event LostFocus] = [Action txtAge_LostFocus($source,$eventArgs)];[Event KeyUp] = [Action txtAge_KeyUp($source,$eventArgs)]"
                                                            Filter="{Binding IntNumberFilter, Mode=OneWay}"
                                                            IsReadOnly="{Binding Path=FormState, ConverterParameter=NONE|READONLY, Mode=OneWay, Converter={StaticResource EnumConverterObj}}"
                                                            Style="{StaticResource MainFrame.TextBox}"
                                                            Text="{Binding CurrentPatient.Age, Mode=TwoWay}" />
                                                        <TextBlock
                                                            Margin="5,0,5,0"
                                                            VerticalAlignment="Center"
                                                            Style="{StaticResource TextBlock.Heading.04}"
                                                            Text="{Binding G0039_G1_Th, Source={StaticResource eHCMSResources}}" />
                                                        <TextBox
                                                            Width="30"
                                                            VerticalContentAlignment="Center"
                                                            IsReadOnly="True"
                                                            Text="{Binding CurrentPatient.MonthsOld, Mode=OneWay}" />
                                                        <TextBlock
                                                            Margin="5,0,5,0"
                                                            VerticalAlignment="Center"
                                                            Style="{StaticResource TextBlock.Heading.04}"
                                                            Text="{Binding N0107_G1_NgSinh, Source={StaticResource eHCMSResources}}"
                                                            TextWrapping="Wrap" />
                                                    </StackPanel>

                                                    <customcontrols:AxDateTextBox
                                                        x:Name="txtDateOfBirth"
                                                        Grid.Row="2"
                                                        Grid.Column="5"
                                                        Width="Auto"
                                                        Height="24"
                                                        VerticalAlignment="Center"
                                                        VerticalContentAlignment="Center"
                                                        cal:Message.Attach="[Event DateChanged] = [Action txtDateOfBirth_DateChanged($source,$eventArgs)]"
                                                        Date="{Binding CurrentPatient.DOBForBaby, Mode=OneWay, ConverterCulture=vi-VN}"
                                                        IsReadOnly="{Binding Path=FormState, ConverterParameter=NONE|READONLY, Mode=OneWay, Converter={StaticResource EnumConverterObj}}"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        UseBothCenturies="True" />
                                                    <TextBlock
                                                        Grid.Row="3"
                                                        Grid.Column="0"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding T1223_G1_GTinh, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <customcontrols:KeyEnabledComboBox
                                                        x:Name="cboGender"
                                                        Grid.Row="3"
                                                        Grid.Column="1"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        DisplayMemberPath="Name"
                                                        IsEnabled="{Binding FormState, ConverterParameter=NEW|EDIT, Mode=OneWay, Converter={StaticResource EnumConverterObj}}"
                                                        ItemsSource="{Binding Genders, Mode=OneWay}"
                                                        SelectedItemEx="{Binding CurrentPatient.GenderObj, Mode=TwoWay, NotifyOnValidationError=True}"
                                                        SelectedValuePath="ID"
                                                        Style="{StaticResource MainFrame.ComboBox}" />
                                                    <TextBlock
                                                        Grid.Row="3"
                                                        Grid.Column="2"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TBlock.Heading.03.RedAsterisk}"
                                                        Text="{Binding Z0045_G1_DauSao, Source={StaticResource eHCMSResources}}" />
                                                    <TextBlock
                                                        Grid.Row="3"
                                                        Grid.Column="4"
                                                        Margin="5,0,5,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding T1703_G1_HonNhan, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <customcontrols:KeyEnabledComboBox
                                                        x:Name="cboMaritalStatus"
                                                        Grid.Row="3"
                                                        Grid.Column="5"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        DisplayMemberPath="ObjectValue"
                                                        IsEnabled="{Binding Path=FormState, ConverterParameter=NEW|EDIT, Mode=OneWay, Converter={StaticResource EnumConverterObj}}"
                                                        ItemsSource="{Binding MaritalStatusList}"
                                                        SelectedValueEx="{Binding CurrentPatient.V_MaritalStatus, Mode=TwoWay}"
                                                        SelectedValuePath="LookupID"
                                                        Style="{StaticResource MainFrame.ComboBox}" />
                                                    <TextBlock
                                                        Grid.Row="4"
                                                        Grid.Column="0"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding N0136_G1_NgheNghiep, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap"
                                                        Visibility="{Binding ApplyReport130, Converter={StaticResource BooleanToNotVisibility}}" />
                                                    <!--<TextBox
                                                        x:Name="txtOccupation"
                                                        Grid.Row="4"
                                                        Grid.Column="1"
                                                        Grid.ColumnSpan="1"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        IsReadOnly="{Binding FormState, ConverterParameter=NONE|READONLY, Converter={StaticResource EnumConverterObj}, Mode=OneWay}"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        Text="{Binding CurrentPatient.PatientOccupation, Mode=TwoWay}" />-->
                                                    <customcontrols:KeyEnabledComboBox
                                                        x:Name="cboJob"
                                                        Grid.Row="4"
                                                        Grid.Column="1"
                                                        Grid.ColumnSpan="1"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        DisplayMemberPath="ObjectValue"
                                                        IsEnabled="{Binding FormState, ConverterParameter=NEW|EDIT, Converter={StaticResource EnumConverterObj}, Mode=OneWay}"
                                                        ItemsSource="{Binding ObjV_Job}"
                                                        SelectedValueEx="{Binding CurrentPatient.V_Job, Mode=TwoWay}"
                                                        SelectedValuePath="LookupID"
                                                        Style="{StaticResource MainFrame.ComboBox}"
                                                        Visibility="{Binding ApplyReport130, Converter={StaticResource BooleanToNotVisibility}}" />
                                                    <TextBlock
                                                        Grid.Row="4"
                                                        Grid.Column="0"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="Nghề nghiệp cấp 3"
                                                        TextWrapping="Wrap"
                                                        Visibility="{Binding ApplyReport130, Converter={StaticResource BooleanToVisibility}}" />
                                                    <customcontrols:AxAutoComplete
                                                        x:Name="AcbJob"
                                                        Grid.Row="4"
                                                        Grid.Column="1"
                                                        Height="22"
                                                        MaxWidth="600"
                                                        cal:Message.Attach="[Event Loaded]=[Action AcbJob_Loaded($source, $eventArgs)];                                                                                                                                                 [Event Populating] = [Action AcbJob_Populating($source, $eventArgs)];                                                             [Event DropDownClosed] = [Action AcbJob_DropDownClosed($source, $eventArgs)]"
                                                        FilterMode="None"
                                                        FontWeight="Bold"
                                                        IsDropDownOpen="True"
                                                        IsEnabled="{Binding IsEnableEdit, Mode=OneWay}"
                                                        MinimumPopulateDelay="300"
                                                        MinimumPrefixLength="0"
                                                        SelectedItem="{Binding SelectedJobParent, Mode=TwoWay}"
                                                        Style="{StaticResource AxAutoCompleteMyStyle}"
                                                        Text="{Binding SelectedJobParent.JobName}"
                                                        TextBoxStyle="{StaticResource MainFrame.TextBox}"
                                                        ValueMemberBinding="{Binding JobName, Mode=TwoWay}"
                                                        Visibility="{Binding ApplyReport130, Converter={StaticResource BooleanToVisibility}}">
                                                        <customcontrols:AxAutoComplete.ItemTemplate>
                                                            <DataTemplate>
                                                                <TextBlock Text="{Binding JobName}" />
                                                            </DataTemplate>
                                                        </customcontrols:AxAutoComplete.ItemTemplate>
                                                    </customcontrols:AxAutoComplete>
                                                    <TextBlock
                                                        Grid.Row="5"
                                                        Grid.Column="0"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="Nghề nghiệp cấp 5"
                                                        TextWrapping="Wrap"
                                                        Visibility="{Binding ApplyReport130, Converter={StaticResource BooleanToVisibility}}" />
                                                    <customcontrols:AxComboBox
                                                        x:Name="cboJobL5"
                                                        Grid.Row="5"
                                                        Grid.Column="1"
                                                        Height="22"
                                                        MaxWidth="600"
                                                        VerticalAlignment="Center"
                                                        cal:Message.Attach="[Event SelectionChanged]=[Action cboJobChild_SelectionChanged($source, $eventArgs)]"
                                                        DisplayMemberPath="JobName"
                                                        IsEnabled="{Binding Path=FormState, ConverterParameter=NEW|EDIT, Mode=OneWay, Converter={StaticResource EnumConverterObj}}"
                                                        IsTabStop="False"
                                                        ItemsSource="{Binding ListJobChild, Mode=OneWay}"
                                                        SelectedItem="{Binding SelectedJobChild, Mode=TwoWay}"
                                                        SelectedValuePath="JobID"
                                                        Style="{StaticResource MainFrame.ComboBox}"
                                                        Visibility="{Binding ApplyReport130, Converter={StaticResource BooleanToVisibility}}" />
                                                    <TextBlock
                                                        Grid.Row="4"
                                                        Grid.Column="4"
                                                        Margin="5,0,5,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding T3390_G1_MaHSo, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <TextBox
                                                        x:Name="txtPatientCode"
                                                        Grid.Row="4"
                                                        Grid.Column="5"
                                                        Grid.ColumnSpan="1"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        VerticalContentAlignment="Center"
                                                        IsEnabled="False"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        Text="{Binding CurrentPatient.FileCodeNumber, Mode=OneWay}" />
                                                    <TextBlock
                                                        Grid.Row="6"
                                                        Grid.Column="0"
                                                        Margin="0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding N0262_G1_NoiLamViec, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <TextBox
                                                        x:Name="txtEmployee"
                                                        Grid.Row="6"
                                                        Grid.Column="1"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        VerticalContentAlignment="Center"
                                                        IsReadOnly="{Binding FormState, ConverterParameter=NONE|READONLY, Converter={StaticResource EnumConverterObj}, Mode=OneWay}"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        Text="{Binding CurrentPatient.PatientEmployer, Mode=TwoWay}" />
                                                    <TextBlock
                                                        Grid.Row="5"
                                                        Grid.Column="4"
                                                        Margin="5,0,5,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding Z2768_G1_NgayBatDauLam, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <customcontrols:AxDateTextBox
                                                        Grid.Row="5"
                                                        Grid.Column="5"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        Text="{Binding CurrentPatient.OccupationDate, Converter={StaticResource ShortDateConverter}, Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                                        UseBothCenturies="True" />
                                                    <TextBlock
                                                        Grid.Row="6"
                                                        Grid.Column="4"
                                                        Margin="5,0,0,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding Q0485_G1_QuocTich, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <customcontrols:AxComboBox
                                                        x:Name="cboNationality"
                                                        Grid.Row="6"
                                                        Grid.Column="5"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        cal:Message.Attach="[Event SelectionChanged]=[Action cboNationality_SelectionChanged($source, $eventArgs)]"
                                                        DisplayMemberPath="NationalityName"
                                                        IsEnabled="{Binding Path=FormState, ConverterParameter=NEW|EDIT, Mode=OneWay, Converter={StaticResource EnumConverterObj}}"
                                                        IsTabStop="False"
                                                        ItemsSource="{Binding Nationalities}"
                                                        SelectedValue="{Binding CurrentPatient.NationalityID, Mode=TwoWay}"
                                                        SelectedValuePath="NationalityID"
                                                        Style="{StaticResource MainFrame.ComboBox}" />
                                                    <TextBlock
                                                        Grid.Row="7"
                                                        Grid.Column="0"
                                                        Margin="0,0,3,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding K2840_G1_DanToc, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <customcontrols:KeyEnabledComboBox
                                                        x:Name="cboEthnic"
                                                        Grid.Row="7"
                                                        Grid.Column="1"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        DisplayMemberPath="ObjectValue"
                                                        IsEnabled="{Binding FormState, ConverterParameter=NEW|EDIT, Converter={StaticResource EnumConverterObj}, Mode=OneWay}"
                                                        ItemsSource="{Binding EthnicsList}"
                                                        SelectedValueEx="{Binding CurrentPatient.V_Ethnic, Mode=TwoWay}"
                                                        SelectedValuePath="LookupID"
                                                        Style="{StaticResource MainFrame.ComboBox}" />
                                                    <TextBlock
                                                        Grid.Row="8"
                                                        Grid.Column="0"
                                                        Margin="0,0,9,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="Passport"
                                                        TextWrapping="Wrap" />
                                                    <TextBox
                                                        Grid.Row="8"
                                                        Grid.Column="1"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        VerticalContentAlignment="Center"
                                                        IsReadOnly="{Binding FormState, ConverterParameter=NONE|READONLY, Converter={StaticResource EnumConverterObj}, Mode=OneWay}"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        Text="{Binding CurrentPatient.Passport, Mode=TwoWay}" />
                                                    <TextBlock
                                                        Grid.Row="7"
                                                        Grid.Column="4"
                                                        Margin="5,0,9,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding Q0485_G1_QuocTich, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <TextBox
                                                        Grid.Row="7"
                                                        Grid.Column="5"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        VerticalContentAlignment="Center"
                                                        IsReadOnly="{Binding FormState, ConverterParameter=NONE|READONLY, Converter={StaticResource EnumConverterObj}, Mode=OneWay}"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        Text="{Binding CurrentPatient.Nationality, Mode=TwoWay}" />
                                                    <TextBlock
                                                        Grid.Row="9"
                                                        Grid.Column="0"
                                                        Margin="0,0,9,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="CMND/CCCD"
                                                        TextWrapping="Wrap" />
                                                    <TextBox
                                                        x:Name="txtIDCard"
                                                        Grid.Row="9"
                                                        Grid.Column="1"
                                                        Height="22"
                                                        VerticalAlignment="Center"
                                                        VerticalContentAlignment="Center"
                                                        IsReadOnly="{Binding FormState, ConverterParameter=NONE|READONLY, Converter={StaticResource EnumConverterObj}, Mode=OneWay}"
                                                        Style="{StaticResource MainFrame.TextBox}"
                                                        Text="{Binding CurrentPatient.IDNumber, Mode=TwoWay}" />
                                                    <TextBlock
                                                        Grid.Row="9"
                                                        Grid.Column="4"
                                                        Margin="5,0,0,0"
                                                        VerticalAlignment="Center"
                                                        Style="{StaticResource TextBlock.Heading.04}"
                                                        Text="{Binding Z2767_G1_NgayCap, Source={StaticResource eHCMSResources}}"
                                                        TextWrapping="Wrap" />
                                                    <customcontrols:AxDateTextBox
                                                        Grid.Row="9"
