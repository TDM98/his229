                            FontSize="12"
                            FontWeight="Bold"
                            Foreground="Crimson"
                            Style="{StaticResource TextBlock.Heading.02}"
                            Text="{Binding TotalCompleteReg}" />
                        <Button
                            Grid.Column="2"
