                Name="txtDrugName"
                HorizontalAlignment="Right"
                Text="{Binding UnitPrice, Converter={StaticResource DecimalConverter}}" />
        </DataTemplate>
        <DataTemplate x:Key="PackagePriceCellEditTemplate">
