                                </StackPanel>
                            </Grid>
                        </Border>

                        <Grid Grid.Column="2" Grid.Row="0" Grid.RowSpan="4"  Visibility="{Binding ElementName=LayoutRoot,Path=DataContext.VisibilityHuy}">
