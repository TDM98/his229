                            <TextBox  Grid.Row="2" Grid.Column="1" Margin="2,1,38,1" d:LayoutOverrides="Width" Style="{StaticResource MainFrame.TextBox}" Text="{Binding ReqNumCode}" IsReadOnly="True" />
