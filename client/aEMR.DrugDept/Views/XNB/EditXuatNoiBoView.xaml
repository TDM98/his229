                            <TextBlock Margin="2,1" Grid.Column="5"   HorizontalAlignment="Right" VerticalAlignment="Center" Text="{Binding S0610_G1_SLgTon, Source={StaticResource eHCMSResources}}" Style="{StaticResource MainFrame.TextBlock.01}"/>
