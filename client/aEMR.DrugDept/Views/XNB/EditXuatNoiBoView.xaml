                        <TextBlock Grid.Row="1" HorizontalAlignment="Left" Margin="2,1,2,1" Grid.Column="3" Text="{Binding G2183_G1_VATx, Source={StaticResource eHCMSResources}}" VerticalAlignment="Center" Style="{StaticResource Title}" />
