                            ItemsSource="{Binding OxygenBreathing, Mode=OneWay}"
                            SelectedItem="{Binding SelectedOxygenBreathing}"
                            SelectedValuePath="LookupID"
                            Style="{StaticResource MainFrame.ComboBox}" />
