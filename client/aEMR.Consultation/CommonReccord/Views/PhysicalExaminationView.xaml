                                CanUserResize="True"
                                CanUserSort="True"
                                ElementStyle="{StaticResource MainFrame.TextBlockCenterAlign}"
                                Header="{Binding Z0431_G1_HApTren, Source={StaticResource eHCMSResources}}"
                                IsReadOnly="True"
                                Visibility="Visible" />
