                            <DataGridTemplateColumn x:Name="Edit" CellTemplate="{StaticResource DTlnkEdit}" />
                            <DataGridTemplateColumn
                                x:Name="Cancel"
                                CellTemplate="{StaticResource DTlnkCancel}"
                                Visibility="Collapsed" />
