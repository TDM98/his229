                            Style="{StaticResource MainFrame.ComboBox}" />
                        <TextBlock
                            Margin="2,0,0,0"
                            VerticalAlignment="Center"
                            Text="{Binding Z3251_G1_MucDoDau, Source={StaticResource eHCMSResources}}" />
