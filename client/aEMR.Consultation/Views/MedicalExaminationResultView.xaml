                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="5*" />
                    <ColumnDefinition Width="1" MinWidth="1" />
                    <ColumnDefinition Width="2*" />
                </Grid.ColumnDefinitions>
                <ContentControl
                    x:Name="UCPatientProfileInfo"
                    Grid.Column="0"
                    HorizontalContentAlignment="Stretch"
                    VerticalContentAlignment="Stretch" />
