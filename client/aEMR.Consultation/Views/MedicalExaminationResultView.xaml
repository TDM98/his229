                    Grid.ColumnSpan="2"
                    MinWidth="200"
                    Margin="0,2"
                    HorizontalAlignment="Left"
                    DisplayMemberPath="ObjectValue"
                    IsEnabled="{Binding EnableByDeptLocationID[3]}"
                    ItemsSource="{Binding V_HealthClassCollection}"
                    SelectedValue="{Binding CurrentMedicalExaminationResult.EarAndThroatDecreases_V_HealthClass}"
                    SelectedValuePath="LookupID"
                    Style="{StaticResource MainFrame.ComboBox}" />

                <TextBlock
                    Grid.Row="27"
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Margin="0,20,0,0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="5. Răng-Hàm-Mặt" />
                <TextBlock
                    Grid.Row="27"
                    Grid.Column="2"
                    Margin="5,20,0,0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="{Binding CurrentMedicalExaminationResult.DentalAndJawResultStaff.FullName, StringFormat='Bác sĩ thực hiện: {0}'}" />

                <TextBlock
                    Grid.Row="28"
                    Grid.Column="0"
                    Margin="0,2,0,0"
                    VerticalAlignment="Top"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="Kết quả khám: " />
                <StackPanel
                    Grid.Row="28"
                    Grid.Column="1"
                    Grid.ColumnSpan="2"
                    Margin="0,2"
                    IsEnabled="{Binding EnableByDeptLocationID[4]}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <TextBlock
                            Grid.Row="0"
                            Grid.Column="0"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Center"
                            Style="{StaticResource TextBlock.Heading.04}"
                            Text="Hàm trên: " />
                        <TextBox
                            Grid.Row="0"
                            Grid.Column="1"
                            Margin="0,2"
                            IsEnabled="{Binding EnableByDeptLocationID[4]}"
                            Style="{StaticResource MainFrame.TextBox}"
                            Text="{Binding CurrentMedicalExaminationResult.UpperJawTestResult}" />
                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="0"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Center"
                            Style="{StaticResource TextBlock.Heading.04}"
                            Text="Hàm dưới: " />
                        <TextBox
                            Grid.Row="1"
                            Grid.Column="1"
                            Margin="0,2"
                            IsEnabled="{Binding EnableByDeptLocationID[4]}"
                            Style="{StaticResource MainFrame.TextBox}"
                            Text="{Binding CurrentMedicalExaminationResult.LowerJawTestResult}" />
                    </Grid>
                </StackPanel>

                <TextBlock
                    Grid.Row="29"
                    Grid.Column="0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="Các bệnh về Răng-Hàm-Mặt (nếu có): " />
                <TextBox
                    Grid.Row="29"
                    Grid.Column="1"
                    Grid.ColumnSpan="2"
                    Margin="0,2"
                    IsEnabled="{Binding EnableByDeptLocationID[4]}"
                    Style="{StaticResource MainFrame.TextBox}"
                    Text="{Binding CurrentMedicalExaminationResult.DentalAndJawDecreases}" />

                <TextBlock
                    Grid.Row="30"
                    Grid.Column="0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="    Phân loại: " />
                <ComboBox
                    Grid.Row="30"
                    Grid.Column="1"
                    Grid.ColumnSpan="2"
                    MinWidth="200"
                    Margin="0,2"
                    HorizontalAlignment="Left"
                    DisplayMemberPath="ObjectValue"
                    IsEnabled="{Binding EnableByDeptLocationID[4]}"
                    ItemsSource="{Binding V_HealthClassCollection}"
                    SelectedValue="{Binding CurrentMedicalExaminationResult.DentalAndJaw_V_HealthClass}"
                    SelectedValuePath="LookupID"
                    Style="{StaticResource MainFrame.ComboBox}" />

                <TextBlock
                    Grid.Row="31"
                    Grid.Column="0"
                    Margin="0,20,0,0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="6. Da liễu" />
                <TextBox
                    Grid.Row="31"
                    Grid.Column="1"
                    Margin="0,22,0,2"
                    IsEnabled="{Binding EnableByDeptLocationID[5]}"
                    Style="{StaticResource MainFrame.TextBox}"
                    Text="{Binding CurrentMedicalExaminationResult.DermatologyTestResult}" />
                <TextBlock
                    Grid.Row="31"
                    Grid.Column="2"
                    Margin="5,20,0,0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="{Binding CurrentMedicalExaminationResult.DermatologyResultStaff.FullName, StringFormat='Bác sĩ thực hiện: {0}'}" />

                <TextBlock
                    Grid.Row="32"
                    Grid.Column="0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="    Phân loại: " />
                <ComboBox
                    Grid.Row="32"
                    Grid.Column="1"
                    Grid.ColumnSpan="2"
                    MinWidth="200"
                    Margin="0,2"
                    HorizontalAlignment="Left"
                    DisplayMemberPath="ObjectValue"
                    IsEnabled="{Binding EnableByDeptLocationID[5]}"
                    ItemsSource="{Binding V_HealthClassCollection}"
                    SelectedValue="{Binding CurrentMedicalExaminationResult.Dermatology_V_HealthClass}"
                    SelectedValuePath="LookupID"
                    Style="{StaticResource MainFrame.ComboBox}" />

                <!--  20200717 TNHX: Thêm chỗ nhập khám sản  -->
                <TextBlock
                    Grid.Row="33"
                    Grid.Column="0"
                    Margin="0,20,0,0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="7. Sản khoa" />
                <TextBlock
                    Grid.Row="33"
                    Grid.Column="2"
                    Margin="5,20,0,0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="{Binding CurrentMedicalExaminationResult.ObstetricResultStaff.FullName, StringFormat='Bác sĩ thực hiện: {0}'}" />
                <ContentControl
                    x:Name="UCObstetricGynecologicalHistory"
                    Grid.Row="34"
                    Grid.Column="0"
                    Grid.ColumnSpan="3"
                    HorizontalContentAlignment="Stretch"
                    VerticalContentAlignment="Stretch"
                    IsEnabled="{Binding EnableByDeptLocationID[6]}" />
                <TextBlock
                    Grid.Row="35"
                    Grid.Column="0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="    Kết quả khám: " />
                <TextBox
                    Grid.Row="35"
                    Grid.Column="1"
                    Margin="0,22,0,2"
                    IsEnabled="{Binding EnableByDeptLocationID[6]}"
                    Style="{StaticResource MainFrame.TextBox}"
                    Text="{Binding CurrentMedicalExaminationResult.ObstetricTestResult}" />

                <TextBlock
                    Grid.Row="36"
                    Grid.Column="0"
                    VerticalAlignment="Center"
                    Style="{StaticResource TextBlock.Heading.04}"
                    Text="    Phân loại: " />
                <ComboBox
                    Grid.Row="36"
                    Grid.Column="1"
                    Grid.ColumnSpan="2"
                    MinWidth="200"
                    Margin="0,2"
                    HorizontalAlignment="Left"
                    DisplayMemberPath="ObjectValue"
                    IsEnabled="{Binding EnableByDeptLocationID[6]}"
                    ItemsSource="{Binding V_HealthClassCollection}"
                    SelectedValue="{Binding CurrentMedicalExaminationResult.Obstetric_V_HealthClass}"
                    SelectedValuePath="LookupID"
                    Style="{StaticResource MainFrame.ComboBox}" />
            </Grid>
        </ScrollViewer>

        <StackPanel
            Grid.Row="4"
            Margin="5"
            HorizontalAlignment="Left"
            Orientation="Horizontal">
            <Button
                x:Name="btnSave"
                MinWidth="80"
                MinHeight="25"
                Content="Lưu"
                IsEnabled="{Binding IsMedicalExamination}"
                Style="{StaticResource MainFrame.Button}" />
            <Button
                x:Name="btnPrint"
                MinWidth="80"
                MinHeight="25"
                Margin="10,0,0,0"
                Content="In"
                IsEnabled="{Binding IsMedicalExamination}"
                Style="{StaticResource MainFrame.Button}" />
            <Button
                x:Name="btnDelete"
                MinWidth="80"
                MinHeight="25"
                Margin="10,0,0,0"
                Content="Xóa"
                IsEnabled="{Binding CanDeleteMedicalExamination}"
                Style="{StaticResource MainFrame.Button}" />
            <Button
                x:Name="btnAgeOfTheArtery"
                MinWidth="80"
                MinHeight="25"
                Margin="20,0,0,0"
                Content="Tính tuổi động mạch"
                IsEnabled="{Binding IsMedicalExamination}"
                Style="{StaticResource MainFrame.Button}" />
        </StackPanel>
    </Grid>
</UserControl>
