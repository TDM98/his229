<UserControl
    x:Class="aEMR.ConsultantEPrescription.Views.CheckMedicalFilesView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Resources="clr-namespace:eHCMSLanguage;assembly=aEMRLanguage"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:converters="clr-namespace:aEMR.Common.Converters;assembly=aEMR.Common"
    xmlns:customcontrols="clr-namespace:aEMR.Controls;assembly=aEMR.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing"
    xmlns:interact="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="1080"
    d:DesignWidth="1920"
    mc:Ignorable="d">
    <UserControl.Resources>
        <converters:StringFormatConverter x:Key="StringFormatConverter" />
        <Resources:eHCMSResources x:Key="eHCMSResources" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <converters:PatientClassConverter x:Key="PatientClassConverter" />
        <converters:FontBoldConverter x:Key="FontBoldConverter" />
        <Style x:Key="CenterText" TargetType="TextBlock">
            <Setter Property="Padding" Value="0,4,0,0" />
        </Style>
    </UserControl.Resources>
    <Border
        x:Name="BorderCanvas"
        Margin="1,1,1,1"
        Style="{StaticResource LightBlueBorderStyle}">
        <ScrollViewer CanContentScroll="True">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="32" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <TextBlock
                    x:Name="TitleForm"
                    Margin="20,0,0,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    FontSize="18"
                    Style="{StaticResource MainFrame.TextBlock.Header.01}"
                    Text="KIỂM DUYỆT HỒ SƠ" />
                <Grid
                    Grid.Row="1"
                    MinWidth="800"
                    MinHeight="500">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="300" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <StackPanel>
                        <customcontrols:AxGrid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="35" />
                                <RowDefinition Height="35" />
                                <RowDefinition Height="35" />
                                <RowDefinition Height="35" />
                                <RowDefinition Height="645" />
                                <!--<RowDefinition Height="200" />-->
                                <RowDefinition Height="35" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="70" />
                                <ColumnDefinition Width="230" />
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontWeight="Bold"
                                Foreground="#6F8AB2"
                                Style="{StaticResource TextBlock.Heading.02}"
                                Text="{Binding T2261_G1_KhoaPh, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <ComboBox
                                x:Name="Departments"
                                Grid.Row="0"
                                Grid.Column="1"
                                Margin="5,2,0,2"
                                DisplayMemberPath="DeptName"
                                ItemsSource="{Binding AllDepartment, Mode=OneWay}"
                                SelectedItem="{Binding SelectedDepartments, Mode=TwoWay}"
                                SelectedValuePath="DeptID"
                                Style="{StaticResource MainFrame.ComboBox}" />

                            <TextBlock
                                Grid.Row="1"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontWeight="Bold"
                                Foreground="#6F8AB2"
                                Style="{StaticResource TextBlock.Heading.02}"
                                Text="Tình trạng" />
                            <customcontrols:AxComboBox
                                Grid.Row="1"
                                Grid.Column="1"
                                MinWidth="224"
                                Margin="5,2,0,2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                cal:Message.Attach="[Event Loaded]=[Action KscV_CheckMedicalFilesStatus_Loaded($source,$eventArgs)]"
                                DisplayMemberPath="ObjectValue"
                                ItemsSource="{Binding V_CheckMedicalFilesStatus}"
                                SelectedValue="{Binding SelectedV_CheckMedicalFilesStatus, Mode=TwoWay}"
                                SelectedValuePath="LookupID"
                                Style="{StaticResource MainFrame.ComboBox}" />
                            <TextBlock
                                Grid.Row="2"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontWeight="Bold"
                                Foreground="#6F8AB2"
                                Text="{Binding G1933_G1_TuNg, StringFormat='{}{0} ', Source={StaticResource eHCMSResources}}" />
                            <DatePicker
                                Name="Dpk_FromDate"
                                Grid.Row="2"
                                Grid.Column="1"
                                Margin="2"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                SelectedDate="{Binding SearchCriteria.FromDate, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                SelectedDateFormat="Short"
                                Style="{StaticResource MainFrame.DatePicker}" />

                            <TextBlock
                                Grid.Row="3"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontWeight="Bold"
                                Foreground="#6F8AB2"
                                Text="{Binding K3192_G1_DenNg, StringFormat='{}{0} ', Source={StaticResource eHCMSResources}}" />
                            <DatePicker
                                Name="Dpk_ToDate"
                                Grid.Row="3"
                                Grid.Column="1"
                                Margin="2"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                SelectedDate="{Binding SearchCriteria.ToDate, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                Style="{StaticResource MainFrame.DatePicker}" />
                            <TextBlock
                                Grid.Row="4"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontWeight="Bold"
                                Foreground="#6F8AB2"
                                Text="{Binding T0914_G1_TenMaBN, StringFormat='{}{0} ', Source={StaticResource eHCMSResources}}" />
                            <customcontrols:AxSearchPatientTextBox
                                x:Name="txtName"
                                Grid.Row="4"
                                Grid.Column="1"
                                Height="22"
                                MinWidth="224"
                                Margin="5,2,0,2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                VerticalContentAlignment="Center"
                                FullName="{Binding SearchCriteria.FullName, Mode=TwoWay}"
                                HICardNumber="{Binding SearchCriteria.InsuranceCard, Mode=TwoWay}"
                                PatientCode="{Binding SearchCriteria.PatientCode, Mode=TwoWay}"
                                PtRegistrationCode="{Binding SearchCriteria.PtRegistrationCode, Mode=TwoWay}"
                                TabIndex="1">
                                <!--<interact:Interaction.Triggers>
                                    <interact:EventTrigger x:Name="myTrigger" EventName="KeyDown">
                                        <customcontrols:TextBoxEnterButtonInvoke x:Name="txtNameInvoker" TargetName="SearchPatientCmd" />
                                    </interact:EventTrigger>
                                </interact:Interaction.Triggers>-->
                            </customcontrols:AxSearchPatientTextBox>

                            <Button
                                x:Name="btnFindRegistration"
                                Grid.Row="5"
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Center"
                                Content="{Binding G1174_G1_TimKiem, Source={StaticResource eHCMSResources}}"
                                Style="{StaticResource MainFrame.Button}"
                                Visibility="{Binding mCheckMedicalFile_Tim, Converter={StaticResource BooleanToVisibility}}" />

                            <CheckBox
                                Grid.Row="5"
                                Grid.ColumnSpan="2"
                                Margin="5,0,3,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                VerticalContentAlignment="Center"
                                Content="BN chưa xuất viện"
                                IsChecked="{Binding SearchCriteria.NotDischarge, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                            <customcontrols:ReadOnlyDataGrid
                                x:Name="grdRequestDetails"
                                Grid.Row="6"
                                Grid.ColumnSpan="2"
                                Height="640"
                                Margin="0,1,0,1"
                                VerticalAlignment="Top"
                                cal:Message.Attach="[Event DblClick]=[Action DoubleClick($source, $eventArgs)];[Event LoadingRow]=[Action grdReqOutwardDetails_LoadingRow($source,$eventArgs)]"
                                AutoGenerateColumns="False"
                                CanUserAddRows="False"
                                CanUserResizeColumns="True"
                                CanUserSortColumns="False"
                                ColumnHeaderStyle="{StaticResource MainFrame.DataGridColumnHeaderStyle}"
                                EnableRowVirtualization="True"
                                HeadersVisibility="All"
                                HorizontalScrollBarVisibility="Auto"
                                IsReadOnly="False"
                                ItemsSource="{Binding CV_CheckMedicalFilesLst}"
                                SelectionMode="Single"
                                SelectionUnit="FullRow"
                                Style="{StaticResource MainFrame.DataGridStyle.OneColor}"
                                VerticalScrollBarVisibility="Visible"
                                VirtualizingPanel.IsVirtualizingWhenGrouping="True">
                                <DataGrid.Columns>
                                    <DataGridTextColumn
                                        x:Name="colPatientCode"
                                        Binding="{Binding Patient.PatientCode}"
                                        CanUserReorder="True"
                                        CanUserResize="True"
                                        CanUserSort="True"
                                        ElementStyle="{StaticResource CenterText}"
                                        Header="{Binding T3274_G1_MaBN, Source={StaticResource eHCMSResources}}"
                                        IsReadOnly="True" />
                                    <DataGridTextColumn
                                        x:Name="colPatientFullName"
                                        MinWidth="150"
                                        Binding="{Binding Patient.FullName}"
                                        CanUserReorder="True"
                                        CanUserResize="True"
                                        CanUserSort="True"
                                        ElementStyle="{StaticResource CenterText}"
                                        Header="{Binding T0834_G1_TenBenhNhan, Source={StaticResource eHCMSResources}}"
                                        IsReadOnly="True" />
                                    <DataGridTextColumn
                                        x:Name="colDateSendRequest"
                                        Width="110"
                                        Binding="{Binding CheckMedicalFiles.DateSendRequest, StringFormat='HH:mm dd/MM/yyyy', Mode=OneWay}"
                                        CanUserReorder="True"
                                        CanUserResize="True"
                                        CanUserSort="True"
                                        ElementStyle="{StaticResource CenterText}"
                                        Header="Ngày gửi hồ sơ"
                                        IsReadOnly="True" />
                                    <DataGridTemplateColumn Header="{Binding K1209_G1_BH, Source={StaticResource eHCMSResources}}">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <CheckBox IsChecked="{Binding PatientClassID, Mode=OneWay, Converter={StaticResource PatientClassConverter}}" IsEnabled="False" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                </DataGrid.Columns>
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow">
                                        <Setter Property="Height" Value="Auto" />
                                        <!--<Setter Property="Margin" Value="1,1,1,1" />-->
                                        <Style.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
                                                <Setter Property="Background" Value="AliceBlue" />
                                                <Setter Property="Foreground" Value="DarkBlue" />
                                                <Setter Property="BorderBrush" Value="Transparent" />
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGrid.RowStyle>
                                <DataGrid.GroupStyle>
                                    <GroupStyle>
                                        <GroupStyle.ContainerStyle>
                                            <Style TargetType="{x:Type GroupItem}">
                                                <!--<Setter Property="Margin" Value="1,1,1,1" />-->
                                                <Setter Property="Template">
                                                    <Setter.Value>
                                                        <ControlTemplate TargetType="{x:Type GroupItem}">
                                                            <Expander
                                                                Background="WhiteSmoke"
                                                                BorderBrush="#FF002255"
                                                                BorderThickness="1,1,1,1"
                                                                Foreground="DarkBlue"
                                                                IsExpanded="True">
                                                                <Expander.Header>
                                                                    <DockPanel>
                                                                        <TextBlock
                                                                            Width="100"
                                                                            Margin="10,0,0,0"
                                                                            VerticalAlignment="Center"
                                                                            FontWeight="Bold"
                                                                            Foreground="DarkBlue"
                                                                            Text="{Binding Path=Name}" />
                                                                        <TextBlock
                                                                            Width="50"
                                                                            Margin="90,0,0,0"
                                                                            VerticalAlignment="Center"
                                                                            FontWeight="Bold"
                                                                            Foreground="DarkRed"
                                                                            Text="{Binding Path=ItemCount}" />
                                                                    </DockPanel>
                                                                </Expander.Header>
                                                                <Expander.Content>
                                                                    <ItemsPresenter />
                                                                </Expander.Content>
                                                            </Expander>
                                                        </ControlTemplate>
                                                    </Setter.Value>
                                                </Setter>
                                            </Style>
                                        </GroupStyle.ContainerStyle>
                                    </GroupStyle>
                                </DataGrid.GroupStyle>
                                <DataGrid.CellStyle>
                                    <Style TargetType="DataGridCell">
                                        <Style.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
                                                <Setter Property="Background" Value="#6DC8FC" />
                                                <Setter Property="Foreground" Value="White" />
                                                <Setter Property="BorderBrush" Value="#6DC8FC" />
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGrid.CellStyle>
                            </customcontrols:ReadOnlyDataGrid>
                            <TextBlock
                                Grid.Row="7"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontWeight="Bold"
                                Foreground="#6F8AB2"
                                Style="{StaticResource TextBlock.Heading.02}"
                                Text="{Binding G1464_G1_Tg, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <TextBlock
                                Grid.Row="7"
                                Grid.Column="1"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                FontWeight="Bold"
                                Foreground="#6F8AB2"
                                Style="{StaticResource TextBlock.Heading.02}"
                                Text="{Binding CountPatientRegistration}" />
                            <Button
                                x:Name="ICDListCmd"
                                Grid.Row="7"
                                Grid.Column="1"
                                Width="100"
                                Height="22"
                                Margin="2,1"
                                HorizontalAlignment="Right"
                                Content="{Binding Z3101_G1_DM_ICD, Source={StaticResource eHCMSResources}}"
                                Style="{StaticResource MainFrame.Button}"
                                Visibility="{Binding mCheckMedicalFile_DanhMucICD, Converter={StaticResource BooleanToVisibility}}" />
                        </customcontrols:AxGrid>
                    </StackPanel>
                    <StackPanel Grid.Column="1" Margin="10,0,0,0">
                        <Grid
                            Grid.Column="1"
                            MinWidth="500"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            ShowGridLines="False">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <TabControl
                                Name="tabCommon"
                                Margin="0,0,0,0"
                                Style="{StaticResource MainFrame.TabControl}">
                                <TabItem Header="Thông tin chung" Style="{StaticResource MainFrame.TabItem}">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="740" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <ScrollViewer CanContentScroll="True">
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="*" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="*" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="32" />
                                                    <RowDefinition Height="*" />
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="32" />
                                                    <ColumnDefinition Width="260" />
                                                    <ColumnDefinition Width="1000" />
                                                </Grid.ColumnDefinitions>
                                                <Border BorderBrush="Black" BorderThickness="1" />
                                                <Border
                                                    Grid.Row="1"
                                                    Grid.RowSpan="10"
                                                    BorderBrush="Black"
                                                    BorderThickness="1,0,1,0" />
                                                <Border
                                                    Grid.Row="1"
                                                    Grid.RowSpan="10"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    BorderBrush="Black"
                                                    BorderThickness="1,0,1,0" />
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="18"
                                                    FontWeight="Bold"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="I"
                                                    TextAlignment="Center" />
                                                <Border
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    BorderBrush="Black"
                                                    BorderThickness="0,1,1,1" />
                                                <TextBlock
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    Margin="5,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    Background="White"
                                                    FontSize="18"
                                                    FontWeight="Bold"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="THÔNG TIN HÀNH CHÍNH"
                                                    TextAlignment="Center" />

                                                <Border
                                                    Grid.Row="1"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    BorderBrush="LightGray"
                                                    BorderThickness="1,0,0,1" />
                                                <TextBlock
                                                    Grid.Row="1"
                                                    Grid.Column="1"
                                                    Margin="5,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="1. Họ và tên"
                                                    TextAlignment="Center" />
                                                <TextBlock
                                                    Grid.Row="1"
                                                    Grid.Column="2"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="{Binding CurrentRegistration.Patient.FullName, Mode=OneWay}"
                                                    TextAlignment="Center" />

                                                <Border
                                                    Grid.Row="2"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    BorderBrush="LightGray"
                                                    BorderThickness="1,0,0,1" />
                                                <TextBlock
                                                    Grid.Row="2"
                                                    Grid.Column="1"
                                                    Margin="5,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="2. Ngày, tháng năm sinh (tuổi)"
                                                    TextAlignment="Center" />
                                                <TextBlock
                                                    Grid.Row="2"
                                                    Grid.Column="2"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="{Binding CurrentRegistration.Patient.DOBText, Mode=OneWay}"
                                                    TextAlignment="Center" />
                                                <TextBlock
                                                    Grid.Row="2"
                                                    Grid.Column="2"
                                                    Margin="70,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="{Binding CurrentRegistration.Patient.AgeString, StringFormat='({0})', Mode=OneWay}"
                                                    TextAlignment="Center" />

                                                <Border
                                                    Grid.Row="3"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    BorderBrush="LightGray"
                                                    BorderThickness="1,0,0,1" />
                                                <TextBlock
                                                    Grid.Row="3"
                                                    Grid.Column="1"
                                                    Margin="5,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="3. Giới tính"
                                                    TextAlignment="Center" />
                                                <TextBlock
                                                    Grid.Row="3"
                                                    Grid.Column="2"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="{Binding CurrentRegistration.Patient.GenderString, Mode=OneWay}"
                                                    TextAlignment="Center" />

                                                <Border
                                                    Grid.Row="4"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    BorderBrush="LightGray"
                                                    BorderThickness="1,0,0,1" />
                                                <TextBlock
                                                    Grid.Row="4"
                                                    Grid.Column="1"
                                                    Margin="5,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="4. Nghề nghiệp"
                                                    TextAlignment="Center" />
                                                <TextBlock
                                                    Grid.Row="4"
                                                    Grid.Column="2"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="{Binding CurrentRegistration.Patient.PatientOccupation, Mode=OneWay}"
                                                    TextAlignment="Center" />

                                                <Border
                                                    Grid.Row="5"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    BorderBrush="LightGray"
                                                    BorderThickness="1,0,0,1" />
                                                <TextBlock
                                                    Grid.Row="5"
                                                    Grid.Column="1"
                                                    Margin="5,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="5. Dân tộc"
                                                    TextAlignment="Center" />
                                                <TextBlock
                                                    Grid.Row="5"
                                                    Grid.Column="2"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="{Binding CurrentRegistration.Patient.EthnicName, Mode=OneWay}"
                                                    TextAlignment="Center" />

                                                <Border
                                                    Grid.Row="6"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    BorderBrush="LightGray"
                                                    BorderThickness="1,0,0,1" />
                                                <TextBlock
                                                    Grid.Row="6"
                                                    Grid.Column="1"
                                                    Margin="5,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="6. Địa chỉ"
                                                    TextAlignment="Center" />
                                                <TextBlock
                                                    Grid.Row="6"
                                                    Grid.Column="2"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="{Binding CurrentRegistration.Patient.PatientFullStreetAddress, Mode=OneWay}"
                                                    TextAlignment="Center" />

                                                <Border
                                                    Grid.Row="7"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    BorderBrush="LightGray"
                                                    BorderThickness="1,0,0,1" />
                                                <TextBlock
                                                    Grid.Row="7"
                                                    Grid.Column="1"
                                                    Margin="5,0,0,0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="7. Nơi làm việc"
                                                    TextAlignment="Center" />
                                                <TextBlock
                                                    Grid.Row="7"
                                                    Grid.Column="2"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    FontSize="13"
                                                    Style="{StaticResource TextBlock.Heading.04}"
                                                    Text="{Binding CurrentRegistration.Patient.PatientEmployer, Mode=OneWay}"
