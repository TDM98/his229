                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding K1558_G1_CanNang, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="5"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding CurPhysicalExamination.Weight}" />

                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="6"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding K1268_G1_BMI2, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="7"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding CurPhysicalExamination.BMI}" />

                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="6"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding T1775_G1_HAp, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="7"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding Pressure}" />

                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="8"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding N0238_G1_NhipTim, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="9"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding CurPhysicalExamination.Pulse}" />

                                            <TextBlock
                                                Grid.Row="1"
