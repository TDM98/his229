                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding N0237_G1_NhipTho, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="9"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding CurPhysicalExamination.RespiratoryRate}" />

                                            <TextBlock
                                                Grid.Row="2"
                                                Grid.Column="8"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding N0234_G1_NhietDo, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="2"
                                                Grid.Column="9"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding CurPhysicalExamination.Temperature, StringFormat='{}{0} °C'}" />

                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="4"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding K1862_G1_ChieuCao, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="5"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding CurPhysicalExamination.Height}" />

                                            <Border
                                                Grid.Row="3"
                                                Grid.ColumnSpan="11"
