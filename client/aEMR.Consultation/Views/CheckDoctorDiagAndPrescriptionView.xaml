<UserControl
    x:Class="aEMR.ConsultantEPrescription.Views.CheckDoctorDiagAndPrescriptionView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Resources="clr-namespace:eHCMSLanguage;assembly=aEMRLanguage"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:converters="clr-namespace:aEMR.Common.Converters;assembly=aEMR.Common"
    xmlns:customcontrols="clr-namespace:aEMR.Controls;assembly=aEMR.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="1080"
    d:DesignWidth="1920"
    mc:Ignorable="d">
    <UserControl.Resources>
        <converters:StringFormatConverter x:Key="StringFormatConverter" />
        <Resources:eHCMSResources x:Key="eHCMSResources" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
        <converters:FontBoldConverter x:Key="FontBoldConverter" />
        <converters:PatientClassConverter x:Key="PatientClassConverter" />

        <DataTemplate x:Key="grdPrescriptionTPL_chk_HI">
            <CheckBox
                Name="chk_HI"
                Margin="0"
                Padding="0"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                IsChecked="{Binding BeOfHIMedicineList, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                IsEnabled="{Binding ElementName=LayoutRoot, Path=DataContext.isHisID, Mode=OneWay}"
                Style="{StaticResource MainFrame.Checkbox.Title1}" />
        </DataTemplate>

        <DataTemplate x:Key="grdPrescriptionTPL_chk_NotInCat">
            <CheckBox
                x:Name="chk_NotInCat"
                Margin="0"
                Padding="0"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                IsChecked="{Binding IsDrugNotInCat, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                Style="{StaticResource MainFrame.Checkbox.Title1}" />
        </DataTemplate>

        <DataTemplate x:Key="grdPrescriptionTPL_SelectedDrugForPrescriptionBrandName">
            <StackPanel Orientation="Horizontal">
                <TextBlock
                    Name="txtDrugName"
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.Auto}"
                    Text="{Binding BrandName}" />
                <TextBlock
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.Auto}"
                    Text="{Binding Z0028_G1_DauNgoacTrai, StringFormat='{} {0}', Source={StaticResource eHCMSResources}}" />
                <TextBlock
                    Name="txtGenericName"
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.Auto}"
                    Text="{Binding RefGenericDrugDetail.GenericName}" />
                <TextBlock
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.Auto}"
                    Text=" " />
                <TextBlock
                    Name="txtStrength"
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.Auto}"
                    Text="{Binding Strength}" />
                <TextBlock
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.Auto}"
                    Text="{Binding Z0027_G1_DauNgoacPhai, StringFormat='{} {0}', Source={StaticResource eHCMSResources}}" />

            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="grdPrescriptionTPL_DrugInstructionNotes_Cell">
            <TextBlock
                MaxHeight="30"
                VerticalAlignment="Center"
                ScrollViewer.VerticalScrollBarVisibility="Auto"
                Text="{Binding DrugInstructionNotes, Mode=OneWay}"
                TextAlignment="Left" />
        </DataTemplate>

        <DataTemplate x:Key="grdPrescriptionTPL_DrugInstructionNotes_CellEditing">
            <customcontrols:AxAutoComplete
                Name="DrugInstructionNotes"
                Height="Auto"
                MinWidth="200"
                MaxHeight="30"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch"
                cal:Message.Attach="[Event Loaded] = [Action DrugInstructionNotes_Loaded($source, $eventArgs)];                 [Event Populating] = [Action DrugInstructionNotes_Populating($source, $eventArgs)];                 [Event LostFocus] = [Action DrugInstructionNotes_LostFocus($source, $eventArgs)];                 [Event DropDownClosed] = [Action DrugInstructionNotes_DropDownClosed($source, $eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                IsTabStop="False"
                IsTextCompletionEnabled="False"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                ItemsSource="{Binding GhiChuAfterFiltering, Mode=OneWay}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="1"
                ScrollViewer.VerticalScrollBarVisibility="Auto"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                Text="{Binding DrugInstructionNotes, Mode=TwoWay}"
                ValueMemberBinding="{Binding NoteDetails}">
                <customcontrols:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Name="txtDrugName"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.Auto}"
                                Text="{Binding NoteDetails}" />
                        </StackPanel>
                    </DataTemplate>
                </customcontrols:AxAutoComplete.ItemTemplate>
            </customcontrols:AxAutoComplete>
        </DataTemplate>
    </UserControl.Resources>
    <Border
        x:Name="BorderCanvas"
        Margin="1,1,1,1"
        Style="{StaticResource LightBlueBorderStyle}">
        <xctk:BusyIndicator BusyContent="{Binding DlgBusyContent}" IsBusy="{Binding DlgIsBusyFlag}">
            <ScrollViewer CanContentScroll="True">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal">
                        <ContentControl
                            x:Name="SearchRegistrationContent"
                            Grid.Row="1"
                            HorizontalContentAlignment="Stretch"
                            VerticalContentAlignment="Stretch" />
                        <TextBlock
                            x:Name="TitleForm"
                            Margin="20,0,0,0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="18"
                            Style="{StaticResource MainFrame.TextBlock.Header.01}"
                            Text="KIỂM TRA LỊCH SỬ KCB CỦA BÁC SĨ" />
                    </StackPanel>
                    <Grid
                        Grid.Row="4"
                        MinWidth="800"
                        MinHeight="500">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="300" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <StackPanel>
                            <customcontrols:AxGrid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="35" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="70" />
                                    <ColumnDefinition Width="215" />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Margin="5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Foreground="#6F8AB2"
                                    Text="{Binding G1933_G1_TuNg, StringFormat='{}{0} ', Source={StaticResource eHCMSResources}}" />
                                <DatePicker
                                    Name="Dpk_FromDate"
                                    Grid.Column="1"
                                    Width="205"
                                    Margin="5,2,0,2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    SelectedDate="{Binding SearchCriteria.FromDate, Mode=TwoWay}"
                                    SelectedDateFormat="Short"
                                    Style="{StaticResource MainFrame.DatePicker}" />

                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Margin="5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Foreground="#6F8AB2"
                                    Text="{Binding K3192_G1_DenNg, StringFormat='{}{0} ', Source={StaticResource eHCMSResources}}" />
                                <DatePicker
                                    Name="Dpk_ToDate"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Width="205"
                                    Margin="5,2,0,2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    SelectedDate="{Binding SearchCriteria.ToDate, Mode=TwoWay}"
                                    Style="{StaticResource MainFrame.DatePicker}" />

                                <TextBlock
                                    Grid.Row="2"
                                    Grid.Column="0"
                                    Margin="5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Foreground="#6F8AB2"
                                    Style="{StaticResource TextBlock.Heading.02}"
                                    Text="{Binding K0961_G1_BSi, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <customcontrols:AxAutoComplete
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Width="205"
                                    Margin="5,2,0,2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    cal:Message.Attach="[Event Loaded] = [Action DoctorStaff_Loaded($source, $eventArgs)];                                 [Event Populating] = [Action DoctorStaff_Populating($source, $eventArgs)];                                 [Event DropDownClosed] = [Action DoctorStaff_DropDownClosed($source, $eventArgs)]"
                                    FilterMode="None"
                                    IsDropDownOpen="True"
                                    IsEnabled="{Binding IsDiag}"
                                    ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                                    MinimumPopulateDelay="300"
                                    MinimumPrefixLength="0"
                                    SelectedItem="{Binding gSelectedDoctorStaff, Mode=TwoWay}"
                                    Style="{StaticResource AxAutoCompleteMyStyle}"
                                    Text="{Binding gSelectedDoctorStaff.FullName}"
                                    ValueMemberBinding="{Binding FullName, Mode=TwoWay}"
                                    ValueMemberPath="FullName" />
                                <TextBlock
                                    Grid.Row="3"
                                    Grid.Column="0"
                                    Margin="5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Foreground="#6F8AB2"
                                    Text="{Binding T0914_G1_TenMaBN, StringFormat='{}{0} ', Source={StaticResource eHCMSResources}}" />
                                <customcontrols:AxSearchPatientTextBox
                                    x:Name="txtName"
                                    Grid.Row="3"
                                    Grid.Column="1"
                                    Width="205"
                                    Height="22"
                                    Margin="5,2,0,2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    VerticalContentAlignment="Center"
                                    FullName="{Binding SearchCriteria.FullName, Mode=TwoWay}"
                                    HICardNumber="{Binding SearchCriteria.InsuranceCard, Mode=TwoWay}"
                                    PatientCode="{Binding SearchCriteria.PatientCode, Mode=TwoWay}"
                                    PtRegistrationCode="{Binding SearchCriteria.PtRegistrationCode, Mode=TwoWay}"
                                    TabIndex="1" />
                                <TextBlock
                                    Grid.Row="4"
                                    Grid.Column="0"
                                    Margin="5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Foreground="#6F8AB2"
                                    Style="{StaticResource TextBlock.Heading.02}"
                                    Text="Tình trạng" />
                                <customcontrols:AxComboBox
                                    Grid.Row="4"
                                    Grid.Column="1"
                                    Width="205"
                                    Margin="5,2,0,2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    cal:Message.Attach="[Event Loaded]=[Action KscV_CheckMedicalFilesStatusForDLS_Loaded($source,$eventArgs)]"
                                    DisplayMemberPath="ObjectValue"
                                    ItemsSource="{Binding V_CheckMedicalFilesStatusForDLS}"
                                    SelectedValue="{Binding SearchCriteria.V_CheckMedicalFilesStatusForDLS, Mode=TwoWay}"
                                    SelectedValuePath="LookupID"
                                    Style="{StaticResource MainFrame.ComboBox}" />

                                <TextBlock
                                    Grid.Row="5"
                                    Grid.Column="0"
                                    Margin="5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Foreground="#6F8AB2"
                                    Style="{StaticResource TextBlock.Heading.02}"
                                    Text="{Binding P0391_G1_PgKham, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <customcontrols:AxComboBox
                                    Grid.Row="5"
                                    Grid.Column="1"
                                    Width="205"
                                    Margin="5,2,0,2"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    cal:Message.Attach="[Event Loaded]=[Action KscLocations_Loaded($source,$eventArgs)]"
                                    DisplayMemberPath="Location.LocationName"
                                    ItemsSource="{Binding Locations}"
                                    SelectedValue="{Binding SelectedLocation, Mode=TwoWay}"
                                    SelectedValuePath="Location.LocationID"
                                    Style="{StaticResource MainFrame.ComboBox}" />
                                <ComboBox
                                    x:Name="Departments"
                                    Grid.Row="5"
                                    Grid.Column="1"
                                    Width="205"
                                    Margin="0,2,0,2"
                                    DisplayMemberPath="Location.LocationName"
                                    ItemsSource="{Binding Locations, Mode=OneWay}"
                                    SelectedValue="{Binding SearchCriteria.DeptLocID, Mode=TwoWay}"
                                    SelectedValuePath="DeptLocationID"
                                    Style="{StaticResource MainFrame.ComboBox}" />

                                <TextBlock
                                    Grid.Row="6"
                                    Grid.Column="0"
                                    Margin="5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Foreground="#6F8AB2"
                                    Style="{StaticResource TextBlock.Heading.02}"
                                    Text="{Binding G1464_G1_Tg, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <TextBlock
                                    Grid.Row="6"
                                    Grid.Column="1"
                                    Margin="5,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontWeight="Bold"
                                    Foreground="#6F8AB2"
                                    Style="{StaticResource TextBlock.Heading.02}"
                                    Text="{Binding CountPatientRegistration}" />
                                
                                <CheckBox Grid.Row="7" Style="{StaticResource MainFrame.Checkbox.Title1}"
                                  Content="BN Chưa duyệt toa Bảo hiểm"
                                  Margin="5,0,0,0" IsChecked="{Binding SearchCriteria.IsConfirmHI, Mode=TwoWay}"
                                  VerticalContentAlignment="Center"
                                  VerticalAlignment="Center"
                                  HorizontalAlignment="Left"
                                  Grid.ColumnSpan="2"/>

                                <Button
                                    x:Name="btnFindRegistration"
                                    Grid.Row="7"
                                    Grid.Column="1"
                                    Margin="5,0,5,0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Content="{Binding G1174_G1_TimKiem, Source={StaticResource eHCMSResources}}"
                                    Style="{StaticResource MainFrame.Button}" />
                            </customcontrols:AxGrid>

                            <customcontrols:ReadOnlyDataGrid
                                Grid.Row="0"
                                Width="auto"
                                Height="678"
                                cal:Message.Attach="[Event DblClick]=[Action DoubleClick($source, $eventArgs)];[Event LoadingRow]=[Action grdPatientRegistration_LoadingRow($source,$eventArgs)]"
                                AutoGenerateColumns="False"
                                ColumnHeaderStyle="{StaticResource MainFrame.DataGridColumnHeaderStyle}"
                                IsReadOnly="True"
                                ItemsSource="{Binding curObjPatientRegistration, Mode=OneWay}"
                                ScrollViewer.HorizontalScrollBarVisibility="Visible"
                                ScrollViewer.VerticalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.DataGridStyle}">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Binding="{Binding Patient.PatientCode}" Header="{Binding T3274_G1_MaBN, Source={StaticResource eHCMSResources}}" />
                                    <DataGridTextColumn
                                        Width="150"
                                        Binding="{Binding Patient.FullName}"
                                        Header="{Binding T0834_G1_TenBenhNhan, Source={StaticResource eHCMSResources}}" />
                                    <DataGridTextColumn
                                        Width="150"
                                        Binding="{Binding MedServiceNames}"
                                        Header="{Binding T0844_G1_TenDV, Source={StaticResource eHCMSResources}}" />
                                    <DataGridTextColumn
                                        Width="150"
                                        Binding="{Binding DischargeDate, StringFormat='HH:mm dd/MM/yyy'}"
                                        Header="Ngày ra viện" />
                                    <DataGridTemplateColumn Header="{Binding K1209_G1_BH, Source={StaticResource eHCMSResources}}">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <CheckBox IsChecked="{Binding PatientClassID, Mode=OneWay, Converter={StaticResource PatientClassConverter}}" IsEnabled="False" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                </DataGrid.Columns>
                            </customcontrols:ReadOnlyDataGrid>
                        </StackPanel>

                        <TabControl
                            Name="tabCommon"
                            Grid.Column="1"
                            Margin="0,0,0,0"
                            Style="{StaticResource MainFrame.TabControl}">
                            <TabItem Header="{Binding G0558_G1_TTinCTiet, Source={StaticResource eHCMSResources}}" Style="{StaticResource MainFrame.TabItem}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="740" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <ScrollViewer MaxHeight="800" CanContentScroll="True">
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition MinHeight="30" MaxHeight="50" />
                                                <RowDefinition MinHeight="30" MaxHeight="50" />
                                                <RowDefinition MinHeight="30" MaxHeight="50" />
                                                <RowDefinition MinHeight="30" />
                                                <RowDefinition MinHeight="30" />
                                                <RowDefinition MinHeight="30" />
                                                <RowDefinition Height="280" />
                                                <RowDefinition Height="400" />
                                                <RowDefinition MinHeight="30" />
                                                <RowDefinition MinHeight="30" />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="120" />
                                                <ColumnDefinition Width="150" />
                                                <ColumnDefinition Width="55" />
                                                <ColumnDefinition Width="50" />
                                                <ColumnDefinition Width="55" />
                                                <ColumnDefinition Width="50" />
                                                <ColumnDefinition Width="55" />
                                                <ColumnDefinition Width="70" />
                                                <ColumnDefinition Width="110" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <Border
                                                Grid.ColumnSpan="11"
                                                BorderBrush="LightGray"
                                                BorderThickness="1,0,0,1" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="0"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding T1584_G1_HoTenBN2, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="1"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding FullName}" />

                                            <Border
                                                Grid.Row="1"
                                                Grid.ColumnSpan="11"
                                                BorderBrush="LightGray"
                                                BorderThickness="1,0,0,1" />
                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="0"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding T3309_G1_MaBN, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding CurPatientRegistation.Patient.PatientCode}" />

                                            <Border
                                                Grid.Row="2"
                                                Grid.ColumnSpan="11"
                                                BorderBrush="LightGray"
                                                BorderThickness="1,0,0,1" />
                                            <TextBlock
                                                Grid.Row="2"
                                                Grid.Column="0"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="Bsi khám" />
                                            <TextBlock
                                                Grid.Row="2"
                                                Grid.Column="1"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding CurPatientRegistation.DiagnosisTreatment.ObjDoctorStaffID.FullName}" />

                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="2"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding T1223_G1_GTinh, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.Column="3"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Foreground="Red"
                                                Text="{Binding GenderString}" />

                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="2"
                                                Margin="5,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Style="{StaticResource TextBlock.Heading.04}"
                                                Text="{Binding G2057_G1_Tuoi, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="3"
                                                Margin="5,0,0,0"
