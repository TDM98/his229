<UserControl
    x:Class="aEMR.ConsultantEPrescription.Views.ConsultationOld_InPtView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:AxControls="clr-namespace:aEMR.Controls;assembly=aEMR.Controls"
    xmlns:Coverters="clr-namespace:aEMR.Common.Converters;assembly=aEMR.Common"
    xmlns:Resources="clr-namespace:eHCMSLanguage;assembly=aEMRLanguage"
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:toolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="600"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Resources:eHCMSResources x:Key="eHCMSResources" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />

        <Coverters:BooleanToVisibilityNotConverter x:Key="BooleanToVisibilityNot" />
        <Coverters:IntConverterMin0 x:Key="IntConverter" />
        <DataTemplate x:Key="ColDeleteTemplate">
            <StackPanel
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Orientation="Horizontal">
                <Button
                    x:Name="lnkDelete"
                    cal:Message.Attach="[Event Click] = [Action lnkDelete_Click($source, $eventArgs)]"
                    ToolTipService.ToolTip="{Binding G2617_G1_Xoa, Source={StaticResource eHCMSResources}}">
                    <Image
                        Width="13"
                        Height="13"
                        Opacity="1"
                        Source="/aEMR.CommonViews;component/Assets/Images/Delete.png" />
                </Button>
            </StackPanel>
        </DataTemplate>

        <DataTemplate x:Key="ColDeleteICD9Template">
            <StackPanel
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Orientation="Horizontal">
                <Button cal:Message.Attach="[Event Click] = [Action lnkDeleteICD9_Click($source, $eventArgs)]" ToolTipService.ToolTip="{Binding G2617_G1_Xoa, Source={StaticResource eHCMSResources}}">
                    <Image
                        Width="13"
                        Height="13"
                        Opacity="1"
                        Source="/aEMR.CommonViews;component/Assets/Images/Delete.png" />
                </Button>
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="ShowICD9Code">
            <StackPanel>
                <TextBlock
                    Name="txtICD9Code"
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.01}"
                    Text="{Binding RefICD9.ICD9Code}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="ICD9Code">
            <AxControls:AxAutoComplete
                MinWidth="400"
                cal:Message.Attach="[Event Loaded] = [Action aucICD9_Code_Loaded($source, $eventArgs)]                                         ;[Event Populating] = [Action aucICD9_Populating($source, $eventArgs)]                                         ;[Event DropDownClosed] = [Action aucICD9_DropDownClosed($source, $eventArgs)]                                         ;[Event DropDownClosing] = [Action aucICD9_DropDownClosing($source, $eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="1"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                ValueMemberBinding="{Binding ICD9Code}"
                ValueMemberPath="ICD9Code">
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Width="auto" Orientation="Horizontal">
                            <TextBlock
                                Name="txtDrugName"
                                Width="50"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding ICD9Code}" />
                            <TextBlock
                                Width="300"
                                Margin="5,0,0,0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding ProcedureName}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>

        <DataTemplate x:Key="Icd10CodeDisplay">
            <TextBlock
                Name="txtDrugName"
                Margin="0"
                Padding="0"
                Style="{StaticResource MainFrame.TextBlock.01}"
                Text="{Binding DiseasesReference.ICD10Code}" />
        </DataTemplate>

        <DataTemplate x:Key="Icd10CodeAcb">
            <!--  ;[Event SelectionChanged] = [Action AutoCompleteBox_SelectionChanged($source, $eventArgs)]  -->
            <!--  Text="{Binding DiseasesReference.ICD10Code,Mode=TwoWay}" LostFocus="AxAutoComplete_LostFocus"  -->
            <AxControls:AxAutoComplete
                MinWidth="400"
                cal:Message.Attach="[Event Loaded] = [Action AcbICD10Code_Loaded($source, $eventArgs)]                                        ;[Event Populating] = [Action aucICD10_Populating($source, $eventArgs)]                                        ;[Event DropDownClosed] = [Action AxAutoComplete_DropDownClosed($source, $eventArgs)]                                        ;[Event DropDownClosing] = [Action AxAutoComplete_DropDownClosing($source, $eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="1"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                ValueMemberBinding="{Binding ICD10Code}"
                ValueMemberPath="ICD10Code">
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Name="txtDrugName"
                                Width="75"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding ICD10Code}" />
                            <TextBlock
                                Width="300"
                                Margin="5,0,0,0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding DiseaseNameVN}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
        <DataTemplate x:Key="Icd10NameDisplay">
            <StackPanel>
                <TextBlock
                    Name="txtDrugName"
                    MinWidth="150"
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.01}"
                    Text="{Binding DiseasesReference.DiseaseNameVN}" />
            </StackPanel>
        </DataTemplate>
        <!--  Text="{Binding DiseasesReference.DiseaseNameVN}"  -->
        <!--  SelectedItem="{Binding DiseasesReference, Mode=TwoWay}"  -->
        <DataTemplate x:Key="Icd10NameAcb">
            <AxControls:AxAutoComplete
                Name="DiseaseName"
                Grid.Row="0"
                Grid.Column="1"
                cal:Message.Attach="[Event Loaded] = [Action AcbICD10Name_Loaded($source, $eventArgs)]                                         ;[Event Populating] = [Action aucICD10Name_Populating($source, $eventArgs)]                                         ;[Event DropDownClosed] = [Action DiseaseName_DropDownClosed($source, $eventArgs)]                                         ;[Event DropDownClosing] = [Action DiseaseName_DropDownClosing($source, $eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="1"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                Text="{Binding DiseasesReference.DiseaseNameVN}"
                ValueMemberBinding="{Binding DiseaseNameVN, Mode=TwoWay}"
                ValueMemberPath="DiseaseNameVN">
                <!--    -->
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel>
                            <TextBlock
                                Name="txtDrugName"
                                MinWidth="150"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding DiseaseNameVN}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>
        <DataTemplate x:Key="ShowProcedureName">
            <StackPanel>
                <TextBlock
                    MinWidth="150"
                    Margin="0"
                    Padding="0"
                    Style="{StaticResource MainFrame.TextBlock.01}"
                    Text="{Binding RefICD9.ProcedureName}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="ProcedureNameTemplate">
            <AxControls:AxAutoComplete
                Name="auProcedureName"
                Grid.Row="0"
                Grid.Column="1"
                cal:Message.Attach="[Event Loaded] = [Action aucICD9_Name_Loaded($source, $eventArgs)]                                         ;[Event Populating] = [Action ProcedureName_Populating($source, $eventArgs)]                                         ;[Event DropDownClosed] = [Action ProcedureName_DropDownClosed($source, $eventArgs)]                                         ;[Event DropDownClosing] = [Action ProcedureName_DropDownClosing($source, $eventArgs)]"
                FilterMode="None"
                IsDropDownOpen="True"
                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                MinimumPopulateDelay="300"
                MinimumPrefixLength="1"
                Style="{StaticResource AxAutoCompleteMyStyle}"
                Text="{Binding RefICD9.ProcedureName}"
                ValueMemberBinding="{Binding ProcedureName, Mode=TwoWay}"
                ValueMemberPath="ProcedureName">
                <!--    -->
                <AxControls:AxAutoComplete.ItemTemplate>
                    <DataTemplate>
                        <StackPanel>
                            <TextBlock
                                MinWidth="150"
                                Margin="0"
                                Padding="0"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding ProcedureName}" />
                        </StackPanel>
                    </DataTemplate>
                </AxControls:AxAutoComplete.ItemTemplate>
            </AxControls:AxAutoComplete>
        </DataTemplate>

        <DataTemplate x:Key="LoadEditStatus">
            <AxControls:KeyEnabledComboBox
                Name="cbxChooseDose"
                Margin="0,1,0,1"
                cal:Message.Attach="[Event Loaded] = [Action UserControl_Loaded($source, $eventArgs)]"
                Background="Transparent"
                BorderBrush="Transparent"
                DisplayMemberPath="ObjectValue"
                SelectedItem="{Binding LookupStatus, Mode=TwoWay}"
                SelectedValuePath="LookupID"
                Style="{StaticResource MainFrame.ComboBox}" />
        </DataTemplate>
        <DataTemplate x:Key="LoadStatus">
            <TextBlock Text="{Binding LookupStatus.ObjectValue}" />
        </DataTemplate>
        <Style TargetType="CheckBox">
            <Setter Property="FlowDirection" Value="LeftToRight" />
            <Setter Property="HorizontalAlignment" Value="Left" />

            <!--<Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <TextBlock
                            Width="{Binding Path=Tag, RelativeSource={RelativeSource AncestorType=CheckBox}}"
                            FlowDirection="LeftToRight"
                            Text="{Binding}"
                            TextWrapping="Wrap" />
                    </DataTemplate>
                </Setter.Value>
            </Setter>-->
        </Style>
    </UserControl.Resources>

    <toolkit:BusyIndicator
        Margin="0,0,0,10"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        DisplayAfter="0:0:0.100"
        IsBusy="{Binding IsProcessing, Mode=OneWay}">
        <toolkit:BusyIndicator.BusyContent>
            <TextBlock
                FontSize="14"
                FontWeight="Bold"
                Text="{Binding Path=StatusText, Mode=OneWay}" />
        </toolkit:BusyIndicator.BusyContent>

        <Grid x:Name="LayoutRoot" Background="White">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>

            <ContentControl
                Grid.Row="0"
                HorizontalContentAlignment="Stretch"
                VerticalContentAlignment="Stretch"
                IsEnabled="{Binding FormEditorIsEnabled, Mode=OneWay}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <!--  PMR Info  -->
                    <Border
                        x:Name="BorderCanvas"
                        Grid.Row="0"
                        Grid.Column="0"
                        Margin="1,1,1,1"
                        Style="{StaticResource LightBlueBorderStyle}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding N0082_G1_NgKham, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <!--<TextBlock Text="{Binding DiagTrmtItem.PatientServiceRecord.ExamDate, StringFormat='dd/MM/yyyy', Mode=TwoWay}"  x:Name="txtExamDate" Margin="0,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" TextWrapping="NoWrap" Grid.Row="0"  Grid.Column="1" Style="{StaticResource TextBlock.Heading.04}" />-->
                                <TextBlock
                                    x:Name="txtExamDate"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding DiagTrmtItem.DiagnosisDate, StringFormat='dd/MM/yyyy HH:mm:ss', Mode=TwoWay}"
                                    TextWrapping="NoWrap" />

                                <TextBlock
                                    Grid.Column="2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding T2692_G1_LoaiCDoanoan, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <ComboBox
                                    Grid.Column="3"
                                    VerticalAlignment="Center"
                                    DisplayMemberPath="ObjectValue"
                                    IsEnabled="False"
                                    ItemsSource="{Binding RefDiagnosis}"
                                    SelectedValue="{Binding DiagTrmtItem.V_DiagnosisType, Mode=TwoWay}"
                                    SelectedValuePath="LookupID"
                                    Style="{StaticResource MainFrame.ComboBox}" />

                                <TextBlock
                                    Grid.Column="4"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding T2222_G1_Khoa, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />

                                <ContentControl
                                    Name="DepartmentContent"
                                    Grid.Column="5"
                                    MinWidth="150"
                                    HorizontalContentAlignment="Stretch"
                                    VerticalContentAlignment="Center" />

                            </Grid>

                            <Line
                                Grid.Row="1"
                                Grid.Column="1"
                                Grid.ColumnSpan="10"
                                Height="1"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Top"
                                Fill="#FFF4F4F5"
                                Stroke="#FFCCCCCC" />

                            <Grid Grid.Row="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="120" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>

                                <TextBlock
                                    Grid.Column="0"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding G0936_G1_TieuDe, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <ComboBox
                                    Name="cboVBehaving"
                                    Grid.Column="1"
                                    VerticalAlignment="Center"
                                    DisplayMemberPath="ObjectValue"
                                    ItemsSource="{Binding RefBehaving}"
                                    SelectedValue="{Binding DiagTrmtItem.PatientServiceRecord.V_Behaving, Mode=TwoWay}"
                                    SelectedValuePath="LookupID"
                                    Style="{StaticResource MainFrame.ComboBox}" />

                                <TextBlock
                                    Grid.Column="2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource MainFrame.TextBlock.01}"
                                    Text="{Binding T3742_G1_MauBAn, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                                <ComboBox
                                    Name="cboPMRTemplate"
                                    Grid.Column="3"
                                    VerticalAlignment="Center"
                                    DisplayMemberPath="TemplateName"
                                    ItemsSource="{Binding RefMedRecTemplate}"
                                    SelectedValue="{Binding DiagTrmtItem.MDRptTemplateID, Mode=TwoWay}"
                                    SelectedValuePath="MDRptTemplateID"
                                    Style="{StaticResource MainFrame.ComboBox}" />

                                <CheckBox
                                    Grid.Column="4"
                                    Grid.ColumnSpan="2"
                                    Margin="61,0,3,0"
                                    HorizontalAlignment="Left"
                                    Content="{Binding Z1328_G1_SuyTim, StringFormat=':{0}', Source={StaticResource eHCMSResources}}"
                                    FlowDirection="RightToLeft"
                                    FontWeight="Bold"
                                    IsChecked="{Binding DiagTrmtItem.HeartFailureType, Mode=TwoWay}"
                                    Visibility="{Binding IsDiagnosisOutHospital, Converter={StaticResource BooleanToVisibility}}" />

                                <CheckBox
                                    Grid.Column="4"
                                    Grid.ColumnSpan="2"
                                    Margin="90,0,3,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Content="{Binding Z3228_G1_BNCOVID, StringFormat=':{0}', Source={StaticResource eHCMSResources}}"
                                    FontWeight="Bold"
                                    IsChecked="{Binding DiagTrmtItem.IsTreatmentCOVID, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                    Visibility="{Binding IsShowCheckCOVID, Converter={StaticResource BooleanToVisibility}}" />
                            </Grid>
                        </Grid>
                    </Border>
                    <Border
                        Grid.Row="0"
                        Grid.Column="1"
                        Margin="1,1,1,1"
                        Style="{StaticResource LightBlueBorderStyle}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding K1373_G1_BSiCDoan, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <!--<TextBlock Text="{Binding DiagTrmtItem.ObjDoctorStaffID.FullName}"   Margin="2,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" TextWrapping="NoWrap" Grid.Row="0"  Grid.Column="4" Style="{StaticResource TextBlock.Heading.04}" />-->
                            <AxControls:AxAutoComplete
                                Grid.Row="0"
                                Grid.Column="4"
                                Width="Auto"
                                Height="22"
                                MinWidth="200"
                                HorizontalAlignment="Stretch"
                                cal:Message.Attach="[Event Populating] = [Action DoctorStaff_Populating($source, $eventArgs)]                                                              ;[Event DropDownClosed] = [Action DoctorStaff_DropDownClosed($source, $eventArgs)]"
                                FilterMode="None"
                                IsDropDownOpen="True"
                                ItemContainerStyle="{StaticResource AxAutoCompleteBoxBkg}"
                                MinimumPopulateDelay="300"
                                MinimumPrefixLength="0"
                                SelectedItem="{Binding gSelectedDoctorStaff, Mode=TwoWay}"
                                Style="{StaticResource AxAutoCompleteMyStyle}"
                                Text="{Binding gSelectedDoctorStaff.FullName}"
                                ValueMemberBinding="{Binding FullName, Mode=TwoWay}"
                                ValueMemberPath="FullName">
                                <AxControls:AxAutoComplete.ItemTemplate>
                                    <DataTemplate>
                                        <StackPanel>
                                            <TextBlock
                                                Margin="0"
                                                Padding="0"
                                                Style="{StaticResource MainFrame.TextBlock.01}"
                                                Text="{Binding FullName}" />
                                        </StackPanel>
                                    </DataTemplate>
                                </AxControls:AxAutoComplete.ItemTemplate>
                            </AxControls:AxAutoComplete>
                            <TextBlock
                                Grid.Row="1"
                                Grid.Column="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource MainFrame.TextBlock.01}"
                                Text="{Binding K3449_G1_DienThoai, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <TextBlock
                                Grid.Row="1"
                                Grid.Column="4"
                                Margin="2,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding DiagTrmtItem.ObjDoctorStaffID.PhoneNumberString}"
                                TextWrapping="NoWrap" />
                        </Grid>
                    </Border>
                    <Border
                        Grid.Row="1"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        Margin="2,1"
                        Style="{StaticResource LightBlueBorderStyle}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="22" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />

                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50*" />
                                <ColumnDefinition Width="50*" />
                            </Grid.ColumnDefinitions>

                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding Z0020_G1_TrieuChungDauHieuLamSang, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <TextBox
                                Name="txtDiagnosisxd"
                                Grid.Row="1"
                                Grid.RowSpan="3"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                TabIndex="1"
                                Text="{Binding DiagTrmtItem.Diagnosis, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                VerticalScrollBarVisibility="Visible" />

                            <!--  Diagnosis  -->
                            <TextBlock
                                Grid.Row="4"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="{Binding K1746_G1_CDoan, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}" />
                            <TextBox
                                Name="txtDiagnosis"
                                Grid.Row="5"
                                Grid.RowSpan="3"
                                Grid.Column="0"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                IsEnabled="{Binding DiagnosisFinalIsEnabled, Mode=OneWay}"
                                Style="{StaticResource MainFrame.TextBox}"
                                TabIndex="1"
                                Text="{Binding DiagTrmtItem.DiagnosisFinal, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                VerticalScrollBarVisibility="Visible" />

                            <!--  Diagnosis Other  -->
                            <TextBlock
                                Grid.Row="8"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Style="{StaticResource TextBlock.Heading.04}"
                                Text="Chẩn đoán khác"
                                TextAlignment="Center"
                                Visibility="{Binding VisibilityOtherDiagnosis, Mode=TwoWay}" />
                            <TextBox
                                Name="txtDiagnosisOther"
                                Grid.Row="9"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                Height="40"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                Text="{Binding DiagTrmtItem.DiagnosisOther, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                Visibility="{Binding VisibilityOtherDiagnosis, Mode=TwoWay}" />

                            <!--  Oriented Treatment  -->
                            <StackPanel
                                Grid.Row="11"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Orientation="Horizontal">
                                <TextBlock
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text="{Binding K0786_G1_1QTrinhBLy, StringFormat='{}{0}:', Source={StaticResource eHCMSResources}}"
                                    Visibility="{Binding IsAdmRequest, Converter={StaticResource BooleanToVisibility}}" />
                                <TextBlock
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Foreground="Red"
                                    Style="{StaticResource TextBlock.Heading.04}"
                                    Text=" *"
                                    Visibility="{Binding IsAdmRequest, Converter={StaticResource BooleanToVisibility}}" />
                            </StackPanel>

                            <TextBox
                                Name="txtOTreatment"
                                Grid.Row="12"
                                Grid.RowSpan="2"
                                Grid.Column="0"
                                MaxHeight="44"
                                Margin="0,2,3,2"
                                AcceptsReturn="True"
                                HorizontalScrollBarVisibility="Auto"
                                Style="{StaticResource MainFrame.TextBox}"
                                TabIndex="3"
                                Text="{Binding DiagTrmtItem.OrientedTreatment, Mode=TwoWay}"
                                TextWrapping="Wrap"
                                VerticalScrollBarVisibility="Visible" />
                            <StackPanel
                                Grid.Row="11"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
