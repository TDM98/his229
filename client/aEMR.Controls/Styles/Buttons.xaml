    <!--  Focus Style  -->
    <Style x:Key="ButtonFocusDefaultVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border x:Name="border"
                            BorderBrush="{DynamicResource ButtonDefaultFocusBorderBrush}"
                            BorderThickness="2"
                            CornerRadius="3"
                            Padding="12,0"
                            RenderTransformOrigin="0.5,0.5" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  Focus Decision Style  -->
    <Style x:Key="ButtonFocusDecisionAdditionMinusVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border x:Name="border"
                            BorderBrush="{DynamicResource ButtonDecisionAdditionMinusFocusBorderBrush}"
                            BorderThickness="2"
                            CornerRadius="3"
                            Padding="12,0"
                            RenderTransformOrigin="0.5,0.5" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  Large Action Style  -->
    <Style x:Key="ButtonLargeActionStyle" TargetType="{x:Type Button}">
        <Setter Property="Height" Value="30" />
        <Setter Property="MinWidth" Value="110" />
        <Setter Property="FocusVisualStyle" Value="{StaticResource ButtonFocusDefaultVisual}" />
        <Setter Property="Foreground" Value="{DynamicResource LabelNormalBrush}" />
        <Setter Property="BorderThickness" Value="2" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="FontFamily" Value="{DynamicResource DefaultAppFontFamily}" />
        <Setter Property="FontSize" Value="{DynamicResource LargeFontSize}" />
        <Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect BlurRadius="2"
                                  Direction="300"
                                  Opacity="0.25"
                                  ShadowDepth="1" />
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="Border"
                            Background="{DynamicResource ButtonLargeActionNormalBackgroundBrush}"
                            CornerRadius="3"
                            Padding="14,2">
                        <Grid>
                            <ContentPresenter x:Name="contentPresenter"
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Center" />
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Border" Property="BorderBrush" Value="{DynamicResource ButtonDefaultFocusBorderBrush}" />
                            <Setter TargetName="Border" Property="Padding" Value="12,0" />
                            <Setter TargetName="Border" Property="BorderThickness" Value="2" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="Border" Property="Background" Value="{DynamicResource ButtonLargeActionPressedBackgroundBrush}" />
                            <Setter TargetName="Border" Property="Padding" Value="14,2" />
                            <Setter TargetName="Border" Property="BorderThickness" Value="0" />
                            <Setter Property="Foreground" Value="{DynamicResource ButtonDefaultPressedForegroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Border" Property="Background" Value="{DynamicResource ButtonLargeActionDisabledBackgroundBrush}" />
                            <Setter TargetName="Border" Property="Padding" Value="14,2" />
                            <Setter TargetName="Border" Property="BorderThickness" Value="0" />
                            <Setter Property="Foreground" Value="{DynamicResource ButtonDefaultDisabledForegroundBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
