        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="SWC_Prim_LTK:AccordionButton">
                    <Grid Margin="{TemplateBinding Padding}" Background="Transparent">
                        <WpfToolkit:VisualStateManager.VisualStateGroups>
